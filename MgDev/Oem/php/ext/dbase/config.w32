ARG_ENABLE("dbase", "Enable the bundled dbase library", "no");

if (PHP_DBASE != "no") {
	EXTENSION("dbase", "dbase.c dbf_head.c dbf_misc.c dbf_ndx.c dbf_rec.c");
	AC_DEFINE('HAVE_DBASE', 1, 'dbase support');
	ADD_FLAG("CFLAGS_DBASE", "/D DBASE=1");
}
