<?xml version="1.0" encoding="utf-8"?>
<test purpose="functions">
  <policies>
    <policy name="boolean"/>
    <default>
      <program>../../../samples/evaluator/eval</program>
    </default>
  </policies>
  <base type="pass">
    <arg>true()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>false()</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>not(true())</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>not(false())</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>not(not(true()))</arg>
    <output>true
</output>
  </base>
</test>