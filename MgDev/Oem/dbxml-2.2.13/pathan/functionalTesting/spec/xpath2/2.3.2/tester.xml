<?xml version="1.0" encoding="utf-8"?>
<test purpose="spec_expressions">
  <policies>
    <policy name="comparison">
      <addArg>test.xml</addArg>
      <addArg>-N test http://www.test.com</addArg>
      <addArg>-N xdt http://www.w3.org/2005/04/xpath-datatypes</addArg>
    </policy>
    <default>
      <program>../../../../samples/evaluator/eval</program>
    </default>
   </policies>
  <base type="pass">
    <arg>data(/test:testcase/@A1)</arg>
    <output>0.0314
</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/@A1) instance of xs:double</arg>
    <output>true</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/@A2)</arg>
    <output>bar
baz
faz</output>
  </base>
  <base type="pass">
    <arg>for $i in data(/test:testcase/@A2) return $i instance of xs:IDREF</arg>
    <output>true
true
true</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E1)</arg>
    <output>1999-05-31</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E1) instance of xdt:untypedAtomic</arg>
    <output>true</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E2)</arg>
    <output>H2O</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E2) instance of xdt:untypedAtomic</arg>
    <output>true</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E3)</arg>
    <output>75.95</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E3) instance of xs:decimal</arg>
    <output>true</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E4)</arg>
    <output>7
8
9</output>
  </base>
  <base type="pass">
    <arg>for $i in data(/test:testcase/test:E4) return $i instance of xs:integer</arg>
    <output>true
true
true</output>
  </base>
  <base type="pass">
    <arg>data(/test:testcase/test:E5)</arg>
    <output>Attempt to get typed value from a complex type with non-mixed complex content</output>
  </base>
</test>