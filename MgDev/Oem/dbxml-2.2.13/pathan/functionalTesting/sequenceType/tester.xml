<?xml version="1.0" encoding="utf-8"?>
<test purpose="Sequence testing">
  <policies>
    <policy name="seq">
      <addArg>-N</addArg>
      <addArg>xs</addArg>
      <addArg>http://www.w3.org/2001/XMLSchema</addArg>
      <addArg>-N</addArg>
      <addArg>xdt</addArg>
      <addArg>http://www.w3.org/2005/04/xpath-datatypes</addArg>
      <addArg>-N</addArg>
      <addArg>baz</addArg>
      <addArg>foofoo</addArg>
      <addArg>test.xml</addArg>
    </policy>
    <default>
      <program>../../samples/evaluator/eval</program>
    </default>
  </policies>
  <base type="fail">
    <arg>() instance of potentiallyEmptySection </arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/emptySection instance of element(*,noElementsType)</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator instance of element(*, xs:decimal)</arg>
    <output>true
</output>
  </base>
  <base type="fail">
    <arg>/rootElement/occurrenceIndicatorIsMin1Max3 instance of element(*,xs:decimal)</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>42.42 instance of xdt:anyAtomicType</arg>
    <output>true
</output>
  </base>
  <base comment="Still investigating if a text node can have a type" type="fail">
    <arg>/rootElement/noOccurrenceIndicator/text() instance of xdt:anyAtomicType</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>12.34 instance of xs:decimal</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/shoes/shoeSize instance of element(*,xs:decimal)</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator instance of element(*,xs:anySimpleType)</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement instance of element(*,xs:anySimpleType)</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/any instance of element(*,xs:anySimpleType)</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>12.23 instance of node()</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>if(/rootElement/noOccurrenceIndicator instance of node()) then true() else false()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator instance of node()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/elementWithAttribute/@attribute instance of node()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator/text() instance of node()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator/text() instance of text()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator instance of text()</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/elementWithAttribute/@attribute/text() instance of text()</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/processing-instruction() instance of processing-instruction()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/comment() instance of comment()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator instance of element()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/noOccurrenceIndicator/text() instance of element()</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/elementWithAttribute/@attribute instance of element()</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/elementWithAttribute/@attribute instance of attribute()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/elementWithAttribute instance of attribute()</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>doc(&quot;test.xml&quot;) instance of document-node()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>doc(&quot;test.xml&quot;) instance of document-node(element(rootElement))</arg>
    <output>true
</output>
  </base>
  <base comment="It complains about a missing type; probably the anonymous type of the root is responsible for thisContextItemExpr," type="pass">
    <arg>doc(&quot;test.xml&quot;) instance of document-node( element(*,shoeSizeType) )</arg>
    <output>false
</output>
  </base>
  <base type="pass">
    <arg>(/) instance of document-node()</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/baz:strElement instance of element(baz:strElement)</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/baz:numElement instance of element(baz:numElement)</arg>
    <output>true
</output>
  </base>
  <base type="pass">
    <arg>/rootElement/baz:numElement instance of element(rootElement)</arg>
    <output>false
</output>
  </base>
</test>