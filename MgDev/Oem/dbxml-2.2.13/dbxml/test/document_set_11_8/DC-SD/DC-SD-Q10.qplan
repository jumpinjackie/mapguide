<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>R(node-element-equality-string,date_of_release,>,'1990-01-01',<,'1995-01-01')</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="catalog" nodeType="element"/>
        <Step axis="child" name="item" nodeType="element"/>
        <DbXmlFilter>
          <Navigation>
            <Step axis="child" name="date_of_release" nodeType="element"/>
            <DbXmlCompare name="greater_than">
              <Sequence>
                <AnyAtomicTypeConstructor value="1990-01-01" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
        <DbXmlFilter>
          <Navigation>
            <Step axis="child" name="date_of_release" nodeType="element"/>
            <DbXmlCompare name="less_than">
              <Sequence>
                <AnyAtomicTypeConstructor value="1995-01-01" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </ForBinding>
    <Sort>
      <Specification modifier="ascending|empty_least">
        <Navigation>
          <Variable name="a"/>
          <Step axis="child" name="publisher" nodeType="element"/>
          <Step axis="child" name="name" nodeType="element">
            <OQPlan>P(node-element-equality-string,prefix,name)</OQPlan>
          </Step>
        </Navigation>
      </Specification>
    </Sort>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <Navigation>
          <Variable name="a"/>
          <Step axis="child" name="title" nodeType="element"/>
        </Navigation>
        <Navigation>
          <Variable name="a"/>
          <Step axis="child" name="publisher" nodeType="element"/>
        </Navigation>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
