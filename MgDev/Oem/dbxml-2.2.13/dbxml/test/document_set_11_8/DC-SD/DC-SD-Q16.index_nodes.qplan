<XQuery>
  <FLWOR>
    <ForBinding name="size">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>U</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="catalog" nodeType="element"/>
        <Step axis="child" name="item" nodeType="element"/>
        <Step axis="child" name="attributes" nodeType="element"/>
        <Step axis="child" name="size_of_book" nodeType="element"/>
      </Navigation>
      <Where>
        <Operator name="greater_than">
          <Operator name="mul">
            <Operator name="mul">
              <Navigation>
                <Variable name="size"/>
                <Step axis="child" name="length" nodeType="element"/>
              </Navigation>
              <Navigation>
                <Variable name="size"/>
                <Step axis="child" name="width" nodeType="element"/>
              </Navigation>
            </Operator>
            <Navigation>
              <Variable name="size"/>
              <Step axis="child" name="height" nodeType="element"/>
            </Navigation>
          </Operator>
          <Sequence>
            <AnyAtomicTypeConstructor value="500000" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
          </Sequence>
        </Operator>
      </Where>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <Navigation>
          <Variable name="size"/>
          <Step axis="parent" uri="*" name="*" nodeType="*"/>
          <Step axis="parent" uri="*" name="*" nodeType="*"/>
          <Step axis="child" name="title" nodeType="element"/>
        </Navigation>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
