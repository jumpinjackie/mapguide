<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>V(node-attribute-equality-string,@id,=,'I6')</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="catalog" nodeType="element"/>
        <Step axis="child" name="item" nodeType="element"/>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="id" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Sequence>
                <AnyAtomicTypeConstructor value="I6" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <Navigation>
          <Variable name="a"/>
          <Step axis="child" name="authors" nodeType="element"/>
          <Step axis="child" name="author" nodeType="element">
            <Predicates>
              <Sequence>
                <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
              </Sequence>
            </Predicates>
          </Step>
          <Step axis="child" name="contact_information" nodeType="element"/>
          <Step axis="child" name="mailing_address" nodeType="element"/>
        </Navigation>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
