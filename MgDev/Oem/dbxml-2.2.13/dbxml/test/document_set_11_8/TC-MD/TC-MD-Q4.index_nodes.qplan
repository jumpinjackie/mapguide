        </Step>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="id" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Sequence>
                <AnyAtomicTypeConstructor value="8" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
        <Step axis="child" name="body" nodeType="element"/>
        <Step axis="child" name="section" nodeType="element"/>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="heading" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Sequence>
                <AnyAtomicTypeConstructor value="introduction" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </ForBinding>
    <ForBinding name="p">
