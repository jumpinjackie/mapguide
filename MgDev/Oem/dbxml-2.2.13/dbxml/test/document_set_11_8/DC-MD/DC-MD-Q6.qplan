<XQuery>
  <FLWOR>
    <ForBinding name="ord">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.1.dbxml">
          <OQPlan>n(P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order),V(node-element-equality-decimal,discount_rate,>,'0.02'))</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="order" nodeType="element"/>
      </Navigation>
      <Where>
        <EveryFLWOR>
          <ForBinding name="item">
            <Navigation>
              <Variable name="ord"/>
              <Step axis="child" name="order_lines" nodeType="element"/>
              <Step axis="child" name="order_line" nodeType="element"/>
            </Navigation>
          </ForBinding>
          <Navigation>
            <Variable name="item"/>
            <Step axis="child" name="discount_rate" nodeType="element"/>
            <DbXmlCompare name="greater_than">
              <Sequence>
                <AnyAtomicTypeConstructor value="0.02" typeuri="http://www.w3.org/2001/XMLSchema" typename="decimal"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </EveryFLWOR>
      </Where>
    </ForBinding>
    <Variable name="ord"/>
  </FLWOR>
</XQuery>
