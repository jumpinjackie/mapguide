<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="bib" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="b">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.8.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bib.xml',n(V(edge-element-equality-string,book.publisher,=,'Addison-Wesley'),V(edge-attribute-equality-decimal,book.@year,>,'1991')))</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="book" nodeType="element">
              <OQPlan>V(edge-attribute-equality-decimal,book.@year,>,'1991')</OQPlan>
            </Step>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="publisher" nodeType="element">
                  <OQPlan>V(edge-element-equality-string,book.publisher,=,'Addison-Wesley')</OQPlan>
                </Step>
                <DbXmlCompare name="equal">
                  <Sequence>
                    <AnyAtomicTypeConstructor value="Addison-Wesley" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                  </Sequence>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
            <DbXmlFilter>
              <Navigation>
                <Step axis="attribute" name="year" nodeType="attribute"/>
                <DbXmlCompare name="greater_than">
                  <Sequence>
                    <AnyAtomicTypeConstructor value="1991" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                  </Sequence>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
        </ForBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <Navigation>
              <Variable name="b"/>
              <Step axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </Step>
            </Navigation>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="book" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="b"/>
              <Step axis="attribute" name="year" nodeType="attribute">
                <OQPlan>P(edge-attribute-equality-decimal,prefix,book.@year)</OQPlan>
              </Step>
            </Navigation>
            <Navigation>
              <Variable name="b"/>
              <Step axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </Step>
            </Navigation>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
