<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="bib" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="book1">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.8.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bib.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="book" nodeType="element"/>
          </Navigation>
        </ForBinding>
        <ForBinding name="book2">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.8.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bib.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="book" nodeType="element"/>
          </Navigation>
          <Where>
            <Operator name="and">
              <Operator name="node-order">
                <Variable name="book1"/>
                <Variable name="book2"/>
              </Operator>
              <Function name="{http://www.w3.org/2005/04/xpath-functions}:not">
                <Function name="{http://www.w3.org/2005/04/xpath-functions}:exists">
                  <Navigation>
                    <Variable name="book2"/>
                    <Step axis="child" name="title" nodeType="element">
                      <RQPlan>V(book.title,=,'[to be calculated]')</RQPlan>
                    </Step>
                    <DbXmlCompare name="equal">
                      <Navigation>
                        <Variable name="book1"/>
                        <Step axis="child" name="title" nodeType="element">
                          <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
                        </Step>
                      </Navigation>
                    </DbXmlCompare>
                  </Navigation>
                </Function>
              </Function>
            </Operator>
          </Where>
        </ForBinding>
        <LetBinding name="aut1">
          <FLWOR>
            <ForBinding name="a">
              <Navigation>
                <Variable name="book1"/>
                <Step axis="child" name="author" nodeType="element"/>
              </Navigation>
            </ForBinding>
            <Sort>
              <Specification modifier="ascending|empty_least">
                <Navigation>
                  <Variable name="a"/>
                  <Step axis="child" name="last" nodeType="element">
                    <OQPlan>P(edge-element-equality-string,prefix,author.last)</OQPlan>
                  </Step>
                </Navigation>
              </Specification>
              <Specification modifier="ascending|empty_least">
                <Navigation>
                  <Variable name="a"/>
                  <Step axis="child" name="first" nodeType="element">
                    <OQPlan>P(edge-element-equality-string,prefix,author.first)</OQPlan>
                  </Step>
                </Navigation>
              </Specification>
            </Sort>
            <Variable name="a"/>
          </FLWOR>
        </LetBinding>
        <LetBinding name="aut2">
          <FLWOR>
            <ForBinding name="a">
              <Navigation>
                <Variable name="book2"/>
                <Step axis="child" name="author" nodeType="element"/>
              </Navigation>
            </ForBinding>
            <Sort>
              <Specification modifier="ascending|empty_least">
                <Navigation>
                  <Variable name="a"/>
                  <Step axis="child" name="last" nodeType="element">
                    <OQPlan>P(edge-element-equality-string,prefix,author.last)</OQPlan>
                  </Step>
                </Navigation>
              </Specification>
              <Specification modifier="ascending|empty_least">
                <Navigation>
                  <Variable name="a"/>
                  <Step axis="child" name="first" nodeType="element">
                    <OQPlan>P(edge-element-equality-string,prefix,author.first)</OQPlan>
                  </Step>
                </Navigation>
              </Specification>
            </Sort>
            <Variable name="a"/>
          </FLWOR>
          <Where>
            <Function name="{http://www.w3.org/2005/04/xpath-functions}:deep-equal">
              <Variable name="aut1"/>
              <Variable name="aut2"/>
            </Function>
          </Where>
        </LetBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="book-pair" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="book1"/>
              <Step axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </Step>
            </Navigation>
            <Navigation>
              <Variable name="book2"/>
              <Step axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </Step>
            </Navigation>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
