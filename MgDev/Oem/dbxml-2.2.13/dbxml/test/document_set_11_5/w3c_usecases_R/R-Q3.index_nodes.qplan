<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="u">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'users.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="user_tuple" nodeType="element"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="rating" nodeType="element"/>
                <DbXmlCompare name="greater_than">
                  <Sequence>
                    <AnyAtomicTypeConstructor value="C" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                  </Sequence>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
        </ForBinding>
        <ForBinding name="i">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="item_tuple" nodeType="element"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="reserve_price" nodeType="element"/>
                <DbXmlCompare name="greater_than">
                  <Sequence>
                    <AnyAtomicTypeConstructor value="1000" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                  </Sequence>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
          <Where>
            <Navigation>
              <Variable name="u"/>
              <Step axis="child" name="userid" nodeType="element">
                <RQPlan>V(user_tuple.userid,=,'[to be calculated]')</RQPlan>
              </Step>
              <DbXmlCompare name="equal">
                <Navigation>
                  <Variable name="i"/>
                  <Step axis="child" name="offered_by" nodeType="element"/>
                </Navigation>
              </DbXmlCompare>
            </Navigation>
          </Where>
        </ForBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="warning" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="u"/>
              <Step axis="child" name="name" nodeType="element"/>
            </Navigation>
            <Navigation>
              <Variable name="u"/>
              <Step axis="child" name="rating" nodeType="element"/>
            </Navigation>
            <Navigation>
              <Variable name="i"/>
              <Step axis="child" name="description" nodeType="element"/>
            </Navigation>
            <Navigation>
              <Variable name="i"/>
              <Step axis="child" name="reserve_price" nodeType="element"/>
            </Navigation>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
