<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="i">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',n(V(node-element-substring-string,description,=,'bic'),V(node-element-substring-string,description,=,'icy'),V(node-element-substring-string,description,=,'cyc'),V(node-element-substring-string,description,=,'ycl'),V(node-element-substring-string,description,=,'cle')))</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="item_tuple" nodeType="element"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="description" nodeType="element">
                  <OQPlan>n(V(node-element-substring-string,description,=,'bic'),V(node-element-substring-string,description,=,'icy'),V(node-element-substring-string,description,=,'cyc'),V(node-element-substring-string,description,=,'ycl'),V(node-element-substring-string,description,=,'cle'))</OQPlan>
                </Step>
                <DbXmlContains>
                  <Sequence>
                    <AnyAtomicTypeConstructor value="Bicycle" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                  </Sequence>
                </DbXmlContains>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
        </ForBinding>
        <LetBinding name="b">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(bid_tuple.itemno,=,'[to be calculated]'))</RQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="bid_tuple" nodeType="element"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="itemno" nodeType="element">
                  <RQPlan>V(bid_tuple.itemno,=,'[to be calculated]')</RQPlan>
                </Step>
                <DbXmlCompare name="equal">
                  <Navigation>
                    <Variable name="i"/>
                    <Step axis="child" name="itemno" nodeType="element">
                      <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
                    </Step>
                  </Navigation>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
        </LetBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <Navigation>
              <Variable name="i"/>
              <Step axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
              </Step>
            </Navigation>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="item_tuple" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="i"/>
              <Step axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
              </Step>
            </Navigation>
            <Navigation>
              <Variable name="i"/>
              <Step axis="child" name="description" nodeType="element"/>
            </Navigation>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="high_bid" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </Name>
              <Children>
                <Function name="{http://www.w3.org/2005/04/xpath-functions}:max">
                  <Navigation>
                    <Variable name="b"/>
                    <Step axis="child" name="bid" nodeType="element"/>
                  </Navigation>
                </Function>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
