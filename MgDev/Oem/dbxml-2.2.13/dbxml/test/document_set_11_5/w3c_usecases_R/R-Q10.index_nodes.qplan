<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="highbid">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="bid_tuple" nodeType="element"/>
          </Navigation>
          <Where>
            <Navigation>
              <Variable name="highbid"/>
              <Step axis="child" name="bid" nodeType="element"/>
              <DbXmlCompare name="equal">
                <Function name="{http://www.w3.org/2005/04/xpath-functions}:max">
                  <Navigation>
                    <QueryPlanFunction result="document" container="11.5.3.dbxml">
                      <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(bid_tuple.itemno,=,'[to be calculated]'))</RQPlan>
                    </QueryPlanFunction>
                    <Step axis="descendant" name="bid_tuple" nodeType="element"/>
                    <DbXmlFilter>
                      <Navigation>
                        <Step axis="child" name="itemno" nodeType="element">
                          <RQPlan>V(bid_tuple.itemno,=,'[to be calculated]')</RQPlan>
                        </Step>
                        <DbXmlCompare name="equal">
                          <Navigation>
                            <Variable name="highbid"/>
                            <Step axis="child" name="itemno" nodeType="element">
                              <OQPlan>P(edge-element-equality-string,prefix,bid_tuple.itemno)</OQPlan>
                            </Step>
                          </Navigation>
                        </DbXmlCompare>
                      </Navigation>
                    </DbXmlFilter>
                    <Step axis="child" name="bid" nodeType="element"/>
                  </Navigation>
                </Function>
              </DbXmlCompare>
            </Navigation>
          </Where>
        </ForBinding>
        <ForBinding name="user">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'users.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="user_tuple" nodeType="element"/>
          </Navigation>
          <Where>
            <Navigation>
              <Variable name="highbid"/>
              <Step axis="child" name="userid" nodeType="element">
                <RQPlan>V(bid_tuple.userid,=,'[to be calculated]')</RQPlan>
              </Step>
              <DbXmlCompare name="equal">
                <Navigation>
                  <Variable name="user"/>
                  <Step axis="child" name="userid" nodeType="element">
                    <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
                  </Step>
                </Navigation>
              </DbXmlCompare>
            </Navigation>
          </Where>
        </ForBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <Navigation>
              <Variable name="highbid"/>
              <Step axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,bid_tuple.itemno)</OQPlan>
              </Step>
            </Navigation>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="high_bid" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="highbid"/>
              <Step axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,bid_tuple.itemno)</OQPlan>
              </Step>
            </Navigation>
            <Navigation>
              <Variable name="highbid"/>
              <Step axis="child" name="bid" nodeType="element"/>
            </Navigation>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="bidder" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </Name>
              <Children>
                <Navigation>
                  <Variable name="user"/>
                  <Step axis="child" name="name" nodeType="element"/>
                  <Step axis="child">
                    <ItemType testType="text"/>
                  </Step>
                </Navigation>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
