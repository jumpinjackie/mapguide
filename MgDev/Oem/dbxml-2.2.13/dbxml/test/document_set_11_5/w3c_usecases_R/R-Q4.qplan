<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="i">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="item_tuple" nodeType="element"/>
          </Navigation>
          <Where>
            <Function name="{http://www.w3.org/2005/04/xpath-functions}:empty">
              <Navigation>
                <QueryPlanFunction result="document" container="11.5.3.dbxml">
                  <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(bid_tuple.itemno,=,'[to be calculated]'))</RQPlan>
                </QueryPlanFunction>
                <Step axis="descendant" name="bid_tuple" nodeType="element"/>
                <DbXmlFilter>
                  <Navigation>
                    <Step axis="child" name="itemno" nodeType="element">
                      <RQPlan>V(bid_tuple.itemno,=,'[to be calculated]')</RQPlan>
                    </Step>
                    <DbXmlCompare name="equal">
                      <Navigation>
                        <Variable name="i"/>
                        <Step axis="child" name="itemno" nodeType="element">
                          <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
                        </Step>
                      </Navigation>
                    </DbXmlCompare>
                  </Navigation>
                </DbXmlFilter>
              </Navigation>
            </Function>
          </Where>
        </ForBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="no_bid_item" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="i"/>
              <Step axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
              </Step>
            </Navigation>
            <Navigation>
              <Variable name="i"/>
              <Step axis="child" name="description" nodeType="element"/>
            </Navigation>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
