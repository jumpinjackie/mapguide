<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="i">
          <Function name="{http://www.w3.org/2005/04/xpath-functions}:distinct-values">
            <Navigation>
              <QueryPlanFunction result="document" container="11.5.3.dbxml">
                <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',P(edge-element-equality-string,prefix,itemno))</OQPlan>
              </QueryPlanFunction>
              <Step axis="descendant" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,itemno)</OQPlan>
              </Step>
            </Navigation>
          </Function>
        </ForBinding>
        <LetBinding name="b">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(bid_tuple.itemno,=,'[to be calculated]'))</RQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="bid_tuple" nodeType="element"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="itemno" nodeType="element">
                  <RQPlan>V(bid_tuple.itemno,=,'[to be calculated]')</RQPlan>
                </Step>
                <DbXmlCompare name="equal">
                  <Variable name="i"/>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
          <Where>
            <Operator name="greater_than_equal">
              <Function name="{http://www.w3.org/2005/04/xpath-functions}:count">
                <Variable name="b"/>
              </Function>
              <Sequence>
                <AnyAtomicTypeConstructor value="3" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
              </Sequence>
            </Operator>
          </Where>
        </LetBinding>
        <LetBinding name="avgbid">
          <Function name="{http://www.w3.org/2005/04/xpath-functions}:avg">
            <Navigation>
              <Variable name="b"/>
              <Step axis="child" name="bid" nodeType="element"/>
            </Navigation>
          </Function>
        </LetBinding>
        <Sort>
          <Specification modifier="descending|empty_least">
            <Variable name="avgbid"/>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="popular_item" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="itemno" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </Name>
              <Children>
                <Variable name="i"/>
              </Children>
            </DOMConstructor>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="avgbid" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </Name>
              <Children>
                <Variable name="avgbid"/>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
