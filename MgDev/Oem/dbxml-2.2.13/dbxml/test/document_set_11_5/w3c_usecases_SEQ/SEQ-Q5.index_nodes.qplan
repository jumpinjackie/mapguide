            <Step axis="descendant">
              <ItemType testType="node"/>
            </Step>
          </Navigation>
          <Variable name="b"/>
        </Operator>
      </Function>
    </Operator>
  </FunctionDefinition>
  <FunctionDefinition name="{http://www.w3.org/2005/04/xquery-local-functions}:follows">
