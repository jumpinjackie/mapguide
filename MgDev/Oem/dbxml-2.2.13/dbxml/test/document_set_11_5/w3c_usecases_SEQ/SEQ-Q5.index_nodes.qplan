            <Step axis="descendant">
              <ItemType testType="node"/>
            </Step>
          </Navigation>
          <Where>
            <Operator name="and">
              <UserFunction name="{http://www.w3.org/2005/04/xquery-local-functions}:follows">
                <Binding name="{}:a">
