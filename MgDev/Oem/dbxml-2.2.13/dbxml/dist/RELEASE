DBXML_VERSION_MAJOR=2
DBXML_VERSION_MINOR=2
DBXML_VERSION_PATCH=13
DBXML_VERSION="$DBXML_VERSION_MAJOR.$DBXML_VERSION_MINOR.$DBXML_VERSION_PATCH"

DBXML_RELEASE_DATE=`date "+%B %e, %Y"`
DBXML_VERSION_STRING="Sleepycat Software: Berkeley DB XML $DBXML_VERSION: ($DBXML_RELEASE_DATE)"

