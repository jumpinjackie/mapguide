XQUERY_VERSION_MAJOR=1
XQUERY_VERSION_MINOR=2
XQUERY_VERSION_PATCH=0
XQUERY_VERSION="$XQUERY_VERSION_MAJOR.$XQUERY_VERSION_MINOR.$XQUERY_VERSION_PATCH"

XQUERY_RELEASE_DATE=`date "+%B %e, %Y"`
XQUERY_VERSION_STRING="Sleepycat Software: Berkeley DB XQuery $XQUERY_VERSION: ($XQUERY_RELEASE_DATE)"
