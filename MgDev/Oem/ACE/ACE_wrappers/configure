#endif

int
main ()
{
  sem_t *s = 0;
  struct timespec tmo;
  int status = 0;
  s = sem_open ("ace_semaphore_foo", O_CREAT, 0600, 1);
  if (s == SEM_FAILED)
    return -1;  /* FAILURE */

  /* Don't care about the time, only whether the call works */
  tmo.tv_sec = 0;
  tmo.tv_nsec = 0;
  if (sem_timedwait (s, &tmo) == -1)
    {
      if (errno == ENOTSUP)
        status = -1;
    }
  else
    sem_post (s);

  sem_unlink ("ace_semaphore_foo");
  sem_close (s);

  return status;
}

