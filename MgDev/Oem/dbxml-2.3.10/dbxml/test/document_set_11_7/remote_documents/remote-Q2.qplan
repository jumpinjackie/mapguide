<XQuery>
  <DbXmlNav>
    <QueryPlanFunction result="document">
      <RQPlan>p("root()")</RQPlan>
      <Sequence>
        <AnyAtomicTypeConstructor value="http://xml.sleepycat.com/intro2xml/prices.xml" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </QueryPlanFunction>
    <Function name="{http://www.w3.org/2005/xpath-functions}:document-uri">
      <TreatAs>
        <ContextItem/>
        <SequenceType occurrence="zero_or_one" testType="node"/>
      </TreatAs>
    </Function>
  </DbXmlNav>
</XQuery>
