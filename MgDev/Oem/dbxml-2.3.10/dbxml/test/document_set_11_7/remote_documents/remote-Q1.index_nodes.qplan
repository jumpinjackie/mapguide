<XQuery>
  <DbXmlNav>
    <QueryPlanFunction result="document">
      <RQPlan>n(p("prices"),p("root()"),p("part"),p(" = "),p("@number"))</RQPlan>
      <Sequence>
        <AnyAtomicTypeConstructor value="http://xml.sleepycat.com/intro2xml/prices.xml" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </QueryPlanFunction>
    <DbXmlStep axis="child" name="prices" nodeType="element">
      <RQPlan>P(root:http://www.sleepycat.com/2002/dbxml.prices)</RQPlan>
    </DbXmlStep>
    <DbXmlStep axis="child" name="part" nodeType="element">
      <RQPlan>P(prices.part)</RQPlan>
    </DbXmlStep>
    <DbXmlFilter>
      <DbXmlNav>
        <DbXmlStep axis="attribute" name="number" nodeType="attribute">
          <RQPlan>P(part.@number)</RQPlan>
        </DbXmlStep>
        <DbXmlCompare name="equal" join="self">
          <RQPlan>V(string,part.@number,=,'150')</RQPlan>
          <Sequence>
            <AnyAtomicTypeConstructor value="150" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
          </Sequence>
        </DbXmlCompare>
      </DbXmlNav>
    </DbXmlFilter>
  </DbXmlNav>
</XQuery>
