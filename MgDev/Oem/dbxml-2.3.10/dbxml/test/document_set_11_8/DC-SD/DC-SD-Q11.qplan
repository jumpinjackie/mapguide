<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>R(node-element-equality-string,date_of_release,>,'1990-01-01',<,'1995-01-01')</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="catalog" nodeType="element"/>
        <DbXmlStep axis="child" name="item" nodeType="element"/>
        <DbXmlFilter>
          <DbXmlCompare name="greater_than" join="child" name="date_of_release" nodeType="element">
            <Sequence>
              <AnyAtomicTypeConstructor value="1990-01-01" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </DbXmlCompare>
        </DbXmlFilter>
        <DbXmlFilter>
          <DbXmlCompare name="less_than" join="child" name="date_of_release" nodeType="element">
            <Sequence>
              <AnyAtomicTypeConstructor value="1995-01-01" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </DbXmlCompare>
        </DbXmlFilter>
      </DbXmlNav>
    </ForBinding>
    <Sort>
      <Specification modifier="descending|empty_least">
        <TreatAs>
          <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="string">
            <Atomize>
              <DbXmlNav>
                <Variable name="a"/>
                <DbXmlStep axis="child" name="date_of_release" nodeType="element"/>
              </DbXmlNav>
            </Atomize>
          </PromoteUntyped>
          <SequenceType occurrence="zero_or_one" testType="anything"/>
        </TreatAs>
      </Specification>
    </Sort>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DbXmlNav>
          <Variable name="a"/>
          <DbXmlStep axis="child" name="title" nodeType="element"/>
        </DbXmlNav>
        <DbXmlNav>
          <Variable name="a"/>
          <DbXmlStep axis="child" name="date_of_release" nodeType="element"/>
        </DbXmlNav>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
