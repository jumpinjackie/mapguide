<XQuery>
  <FLWOR>
    <ForBinding name="item">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>n(P(node-element-equality-string,prefix,item_id),V(node-attribute-equality-string,@id,=,'I7'))</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="catalog" nodeType="element"/>
        <DbXmlStep axis="child" name="item" nodeType="element"/>
        <DbXmlFilter>
          <DbXmlCompare name="equal" join="attribute" name="id" nodeType="attribute">
            <Sequence>
              <AnyAtomicTypeConstructor value="I7" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </DbXmlCompare>
        </DbXmlFilter>
      </DbXmlNav>
    </ForBinding>
    <ForBinding name="related">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>V(node-attribute-equality-string,@id,=,'[to be calculated]')</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="catalog" nodeType="element"/>
        <DbXmlStep axis="child" name="item" nodeType="element"/>
      </DbXmlNav>
      <Where>
        <DocumentOrder>
          <DbXmlNav>
            <Variable name="item"/>
            <DbXmlStep axis="child" name="related_items" nodeType="element"/>
            <DbXmlStep axis="child" name="related_item" nodeType="element"/>
            <DbXmlCompare name="equal" join="child" name="item_id" nodeType="element">
              <OQPlan>V(node-element-equality-string,item_id,=,'[to be calculated]')</OQPlan>
              <Atomize>
                <DbXmlNav>
                  <Variable name="related"/>
                  <DbXmlStep axis="attribute" name="id" nodeType="attribute"/>
                </DbXmlNav>
              </Atomize>
            </DbXmlCompare>
          </DbXmlNav>
        </DocumentOrder>
      </Where>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DbXmlNav>
          <Variable name="related"/>
          <DbXmlStep axis="child" name="title" nodeType="element"/>
        </DbXmlNav>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
