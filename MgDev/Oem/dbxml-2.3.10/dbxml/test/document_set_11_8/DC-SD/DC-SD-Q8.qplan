<XQuery>
  <DbXmlNav>
    <QueryPlanFunction result="collection" container="11.8.2.dbxml">
      <OQPlan>V(node-attribute-equality-string,@id,=,'I4')</OQPlan>
    </QueryPlanFunction>
    <DbXmlStep axis="child" name="catalog" nodeType="element"/>
    <DbXmlStep axis="child" uri="*" name="*" nodeType="element"/>
    <DbXmlFilter>
      <DbXmlCompare name="equal" join="attribute" name="id" nodeType="attribute">
        <Sequence>
          <AnyAtomicTypeConstructor value="I4" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </DbXmlCompare>
    </DbXmlFilter>
    <DbXmlStep axis="child" name="publisher" nodeType="element"/>
  </DbXmlNav>
</XQuery>
