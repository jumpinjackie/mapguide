<XQuery>
  <FLWOR>
    <LetBinding name="item">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>V(node-attribute-equality-string,@id,=,'I2')</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="catalog" nodeType="element"/>
        <DbXmlStep axis="child" name="item" nodeType="element"/>
        <DbXmlFilter>
          <DbXmlCompare name="equal" join="attribute" name="id" nodeType="attribute">
            <Sequence>
              <AnyAtomicTypeConstructor value="I2" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </DbXmlCompare>
        </DbXmlFilter>
      </DbXmlNav>
    </LetBinding>
    <ForBinding name="prevItem">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>U</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="catalog" nodeType="element"/>
        <Predicate>
          <DbXmlNav>
            <DbXmlStep axis="child" name="item" nodeType="element"/>
            <DbXmlFilter>
              <Operator name="node-order">
                <TreatAs>
                  <ContextItem/>
                  <SequenceType occurrence="zero_or_one" testType="node"/>
                </TreatAs>
                <TreatAs>
                  <Variable name="item"/>
                  <SequenceType occurrence="zero_or_one" testType="node"/>
                </TreatAs>
              </Operator>
            </DbXmlFilter>
          </DbXmlNav>
          <Operator name="equal">
            <Function name="{http://www.w3.org/2005/xpath-functions}:position"/>
            <Function name="{http://www.w3.org/2005/xpath-functions}:last"/>
          </Operator>
        </Predicate>
      </DbXmlNav>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="CurrentItem" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DocumentOrder>
              <DbXmlNav>
                <Variable name="item"/>
                <DbXmlStep axis="attribute" name="id" nodeType="attribute"/>
              </DbXmlNav>
            </DocumentOrder>
          </Children>
        </DOMConstructor>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="PreviousItem" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DbXmlNav>
              <Variable name="prevItem"/>
              <DbXmlStep axis="attribute" name="id" nodeType="attribute">
                <OQPlan>P(node-attribute-equality-string,prefix,@id)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
          </Children>
        </DOMConstructor>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
