<XQuery>
  <DbXmlNav>
    <QueryPlanFunction result="collection" container="11.8.2.dbxml">
      <OQPlan>V(node-attribute-equality-string,@id,=,'I3')</OQPlan>
    </QueryPlanFunction>
    <DbXmlStep axis="child" name="catalog" nodeType="element"/>
    <DbXmlStep axis="child" name="item" nodeType="element"/>
    <DbXmlFilter>
      <DbXmlCompare name="equal" join="attribute" name="id" nodeType="attribute">
        <Sequence>
          <AnyAtomicTypeConstructor value="I3" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </DbXmlCompare>
    </DbXmlFilter>
    <DbXmlStep axis="child" name="authors" nodeType="element"/>
    <Predicate>
      <DbXmlStep axis="child" name="author" nodeType="element"/>
      <Sequence>
        <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
      </Sequence>
    </Predicate>
  </DbXmlNav>
</XQuery>
