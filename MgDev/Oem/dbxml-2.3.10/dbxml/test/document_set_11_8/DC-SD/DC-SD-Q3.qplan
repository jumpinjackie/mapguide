<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <Function name="{http://www.w3.org/2005/xpath-functions}:distinct-values">
        <Atomize>
          <DbXmlNav>
            <QueryPlanFunction result="collection" container="11.8.2.dbxml">
              <OQPlan>n(P(node-element-equality-string,prefix,name),R(node-element-equality-string,date_of_release,>=,'1990-01-01',<,'1991-01-01'))</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="child" name="catalog" nodeType="element"/>
            <DbXmlStep axis="child" name="item" nodeType="element"/>
            <DbXmlFilter>
              <DbXmlCompare name="greater_than_equal" join="child" name="date_of_release" nodeType="element">
                <Sequence>
                  <AnyAtomicTypeConstructor value="1990-01-01" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </DbXmlCompare>
            </DbXmlFilter>
            <DbXmlFilter>
              <DbXmlCompare name="less_than" join="child" name="date_of_release" nodeType="element">
                <Sequence>
                  <AnyAtomicTypeConstructor value="1991-01-01" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </DbXmlCompare>
            </DbXmlFilter>
            <DbXmlStep axis="child" name="publisher" nodeType="element"/>
            <DbXmlStep axis="child" name="name" nodeType="element"/>
          </DbXmlNav>
        </Atomize>
      </Function>
    </ForBinding>
    <LetBinding name="b">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>V(node-element-equality-string,name,=,'[to be calculated]')</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="catalog" nodeType="element"/>
        <DbXmlStep axis="child" name="item" nodeType="element"/>
        <DbXmlStep axis="child" name="publisher" nodeType="element"/>
        <DbXmlFilter>
          <DbXmlCompare name="equal" join="child" name="name" nodeType="element">
            <Variable name="a"/>
          </DbXmlCompare>
        </DbXmlFilter>
      </DbXmlNav>
    </LetBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="Publisher" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <Variable name="a"/>
          </Children>
        </DOMConstructor>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="NumberOfItems" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <Function name="{http://www.w3.org/2005/xpath-functions}:count">
              <Variable name="b"/>
            </Function>
          </Children>
        </DOMConstructor>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
