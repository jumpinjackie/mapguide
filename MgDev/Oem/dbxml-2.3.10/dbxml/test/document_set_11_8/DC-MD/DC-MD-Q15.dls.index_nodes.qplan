<XQuery>
  <FLWOR>
    <ForBinding name="order">
      <DbXmlNav>
        <LookupIndex container="11.8.1.dbxml">
          <ValueQP index="edge-attribute-equality-string" operation="eq" parent="order" child="id" value="7"/>
        </LookupIndex>
        <Join type="parent-of-attribute" return="argument">
          <DbXmlNav>
            <QueryPlanFunction result="collection" container="11.8.1.dbxml">
              <OQPlan>n(V(edge-attribute-equality-string,order.@id,=,'7'),P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order))</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="child" name="order" nodeType="element">
              <OQPlan>P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order)</OQPlan>
            </DbXmlStep>
          </DbXmlNav>
        </Join>
      </DbXmlNav>
    </ForBinding>
    <ForBinding name="cust">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.1.dbxml">
          <OQPlan>V(edge-attribute-equality-string,customer.@id,=,'[to be calculated]')</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="customers" nodeType="element"/>
        <DbXmlStep axis="child" name="customer" nodeType="element"/>
      </DbXmlNav>
      <Where>
        <DbXmlNav>
          <Variable name="order"/>
          <DbXmlCompare name="equal" join="child" name="customer_id" nodeType="element">
            <OQPlan>V(node-element-equality-string,customer_id,=,'[to be calculated]')</OQPlan>
            <Atomize>
              <DbXmlNav>
                <Variable name="cust"/>
                <DbXmlStep axis="attribute" name="id" nodeType="attribute">
                  <OQPlan>P(edge-attribute-equality-string,prefix,customer.@id)</OQPlan>
                </DbXmlStep>
              </DbXmlNav>
            </Atomize>
          </DbXmlCompare>
        </DbXmlNav>
      </Where>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DbXmlNav>
          <Variable name="order"/>
          <DbXmlStep axis="attribute" name="id" nodeType="attribute">
            <OQPlan>P(edge-attribute-equality-string,prefix,order.@id)</OQPlan>
          </DbXmlStep>
        </DbXmlNav>
        <DbXmlNav>
          <Variable name="order"/>
          <DbXmlStep axis="child" name="order_status" nodeType="element"/>
        </DbXmlNav>
        <DbXmlNav>
          <Variable name="cust"/>
          <DbXmlStep axis="child" name="first_name" nodeType="element"/>
        </DbXmlNav>
        <DbXmlNav>
          <Variable name="cust"/>
          <DbXmlStep axis="child" name="last_name" nodeType="element"/>
        </DbXmlNav>
        <DbXmlNav>
          <Variable name="cust"/>
          <DbXmlStep axis="child" name="phone_number" nodeType="element"/>
        </DbXmlNav>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
