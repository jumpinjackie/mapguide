<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.1.dbxml">
          <OQPlan>n(P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order),V(node-element-equality-double,total,>,'11000'))</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="order" nodeType="element"/>
        <DbXmlFilter>
          <DbXmlCompare name="greater_than" join="child" name="total" nodeType="element">
            <Sequence>
              <AnyAtomicTypeConstructor value="11000.0" typeuri="http://www.w3.org/2001/XMLSchema" typename="decimal"/>
            </Sequence>
          </DbXmlCompare>
        </DbXmlFilter>
      </DbXmlNav>
    </ForBinding>
    <Sort>
      <Specification modifier="descending|empty_least">
        <TreatAs>
          <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="string">
            <Atomize>
              <DbXmlNav>
                <Variable name="a"/>
                <DbXmlStep axis="child" name="total" nodeType="element"/>
              </DbXmlNav>
            </Atomize>
          </PromoteUntyped>
          <SequenceType occurrence="zero_or_one" testType="anything"/>
        </TreatAs>
      </Specification>
    </Sort>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DbXmlNav>
          <Variable name="a"/>
          <DbXmlStep axis="attribute" name="id" nodeType="attribute">
            <OQPlan>P(edge-attribute-equality-string,prefix,order.@id)</OQPlan>
          </DbXmlStep>
        </DbXmlNav>
        <DbXmlNav>
          <Variable name="a"/>
          <DbXmlStep axis="child" name="order_date" nodeType="element"/>
        </DbXmlNav>
        <DbXmlNav>
          <Variable name="a"/>
          <DbXmlStep axis="child" name="total" nodeType="element"/>
        </DbXmlNav>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
