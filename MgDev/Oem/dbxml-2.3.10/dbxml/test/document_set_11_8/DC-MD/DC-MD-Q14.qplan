<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.1.dbxml">
          <OQPlan>n(V(node-element-substring-string,biography,substring,'hockey'),P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.authors))</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="authors" nodeType="element"/>
        <DbXmlStep axis="child" name="author" nodeType="element"/>
        <DbXmlFilter>
          <DbXmlContains join="child" name="biography" nodeType="element">
            <Sequence>
              <AnyAtomicTypeConstructor value="hockey" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </DbXmlContains>
        </DbXmlFilter>
      </DbXmlNav>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DbXmlNav>
          <Variable name="a"/>
          <DbXmlStep axis="attribute" name="id" nodeType="attribute">
            <OQPlan>P(edge-attribute-equality-string,prefix,author.@id)</OQPlan>
          </DbXmlStep>
        </DbXmlNav>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
