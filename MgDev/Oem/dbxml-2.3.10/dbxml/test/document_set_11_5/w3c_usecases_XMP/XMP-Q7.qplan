<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="bib" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="b">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.8.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bib.xml',n(V(edge-element-equality-string,book.publisher,=,'Addison-Wesley'),V(edge-attribute-equality-double,book.@year,>,'1991')))</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="book" nodeType="element"/>
            <DbXmlFilter>
              <DbXmlCompare name="equal" join="child" name="publisher" nodeType="element">
                <OQPlan>V(edge-element-equality-string,book.publisher,=,'Addison-Wesley')</OQPlan>
                <Sequence>
                  <AnyAtomicTypeConstructor value="Addison-Wesley" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </DbXmlCompare>
            </DbXmlFilter>
            <DbXmlFilter>
              <DbXmlCompare name="greater_than" join="attribute" name="year" nodeType="attribute">
                <OQPlan>V(edge-attribute-equality-double,book.@year,>,'1991')</OQPlan>
                <Sequence>
                  <AnyAtomicTypeConstructor value="1991" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                </Sequence>
              </DbXmlCompare>
            </DbXmlFilter>
          </DbXmlNav>
        </ForBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <TreatAs>
              <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="string">
                <Atomize>
                  <DbXmlNav>
                    <Variable name="b"/>
                    <DbXmlStep axis="child" name="title" nodeType="element">
                      <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
                    </DbXmlStep>
                  </DbXmlNav>
                </Atomize>
              </PromoteUntyped>
              <SequenceType occurrence="zero_or_one" testType="anything"/>
            </TreatAs>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="book" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DbXmlNav>
              <Variable name="b"/>
              <DbXmlStep axis="attribute" name="year" nodeType="attribute">
                <OQPlan>P(edge-attribute-equality-double,prefix,book.@year)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
            <DbXmlNav>
              <Variable name="b"/>
              <DbXmlStep axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
