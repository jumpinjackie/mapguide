<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="u">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'users.xml',U)</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="user_tuple" nodeType="element"/>
          </DbXmlNav>
        </ForBinding>
        <LetBinding name="b">
          <DbXmlNav>
            <LookupIndex container="11.5.3.dbxml">
              <DocumentQP index="node-metadata-equality-string" operation="eq" child="name:http://www.sleepycat.com/2002/dbxml" value="bids.xml">
                <ValueQP index="edge-element-equality-string" operation="eq" parent="bid_tuple" child="userid">
                  <Atomize>
                    <DbXmlNav>
                      <Variable name="u"/>
                      <DbXmlStep axis="child" name="userid" nodeType="element">
                        <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
                      </DbXmlStep>
                    </DbXmlNav>
                  </Atomize>
                </ValueQP>
              </DocumentQP>
            </LookupIndex>
            <Join type="parent-of-child" return="argument">
              <DbXmlNav>
                <QueryPlanFunction result="document" container="11.5.3.dbxml">
                  <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',P(edge-element-equality-string,prefix,bid_tuple.userid))</OQPlan>
                </QueryPlanFunction>
                <DbXmlStep axis="descendant" name="bid_tuple" nodeType="element"/>
              </DbXmlNav>
            </Join>
          </DbXmlNav>
        </LetBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <TreatAs>
              <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="string">
                <Atomize>
                  <DbXmlNav>
                    <Variable name="u"/>
                    <DbXmlStep axis="child" name="userid" nodeType="element">
                      <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
                    </DbXmlStep>
                  </DbXmlNav>
                </Atomize>
              </PromoteUntyped>
              <SequenceType occurrence="zero_or_one" testType="anything"/>
            </TreatAs>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="user" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DbXmlNav>
              <Variable name="u"/>
              <DbXmlStep axis="child" name="userid" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
            <DbXmlNav>
              <Variable name="u"/>
              <DbXmlStep axis="child" name="name" nodeType="element"/>
            </DbXmlNav>
            <If>
              <Test>
                <Function name="{http://www.w3.org/2005/xpath-functions}:empty">
                  <Variable name="b"/>
                </Function>
              </Test>
              <Then>
                <DOMConstructor type="element">
                  <Name>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="status" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                    </Sequence>
                  </Name>
                  <Children>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="inactive" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                    </Sequence>
                  </Children>
                </DOMConstructor>
              </Then>
              <Else>
                <DOMConstructor type="element">
                  <Name>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="status" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                    </Sequence>
                  </Name>
                  <Children>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="active" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                    </Sequence>
                  </Children>
                </DOMConstructor>
              </Else>
            </If>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
