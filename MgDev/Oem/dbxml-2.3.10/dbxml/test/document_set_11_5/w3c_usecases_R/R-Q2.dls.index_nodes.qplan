          <Children>
            <DbXmlNav>
              <Variable name="i"/>
              <DbXmlStep axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
            <DbXmlNav>
              <Variable name="i"/>
              <DbXmlStep axis="child" name="description" nodeType="element"/>
