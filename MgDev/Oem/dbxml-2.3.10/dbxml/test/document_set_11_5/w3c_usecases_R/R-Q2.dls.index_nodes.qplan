    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="i">
          <DbXmlNav>
            <LookupIndex container="11.5.3.dbxml">
              <DocumentQP index="node-metadata-equality-string" operation="eq" child="name:http://www.sleepycat.com/2002/dbxml" value="items.xml">
                <ValueQP index="node-element-substring-string" operation="substring" child="description" value="Bicycle"/>
