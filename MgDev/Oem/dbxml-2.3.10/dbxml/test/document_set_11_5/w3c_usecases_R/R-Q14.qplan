<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="i">
          <Function name="{http://www.w3.org/2005/xpath-functions}:distinct-values">
            <Atomize>
              <DbXmlNav>
                <QueryPlanFunction result="document" container="11.5.3.dbxml">
                  <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',P(edge-element-equality-string,prefix,itemno))</OQPlan>
                </QueryPlanFunction>
                <DbXmlStep axis="descendant" name="itemno" nodeType="element">
                  <OQPlan>P(edge-element-equality-string,prefix,itemno)</OQPlan>
                </DbXmlStep>
              </DbXmlNav>
            </Atomize>
          </Function>
        </ForBinding>
        <LetBinding name="b">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(edge-element-equality-string,bid_tuple.itemno,=,'[to be calculated]'))</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="bid_tuple" nodeType="element"/>
            <DbXmlFilter>
              <DbXmlCompare name="equal" join="child" name="itemno" nodeType="element">
                <OQPlan>V(edge-element-equality-string,bid_tuple.itemno,=,'[to be calculated]')</OQPlan>
                <Variable name="i"/>
              </DbXmlCompare>
            </DbXmlFilter>
          </DbXmlNav>
          <Where>
            <Operator name="greater_than_equal">
              <Function name="{http://www.w3.org/2005/xpath-functions}:count">
                <Variable name="b"/>
              </Function>
              <Sequence>
                <AnyAtomicTypeConstructor value="3" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
              </Sequence>
            </Operator>
          </Where>
        </LetBinding>
        <LetBinding name="avgbid">
          <Function name="{http://www.w3.org/2005/xpath-functions}:avg">
            <Atomize>
              <DocumentOrder>
                <DbXmlNav>
                  <Variable name="b"/>
                  <DbXmlStep axis="child" name="bid" nodeType="element"/>
                </DbXmlNav>
              </DocumentOrder>
            </Atomize>
          </Function>
        </LetBinding>
        <Sort>
          <Specification modifier="descending|empty_least">
            <TreatAs>
              <Variable name="avgbid"/>
              <SequenceType occurrence="zero_or_one" testType="anything"/>
            </TreatAs>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="popular_item" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="itemno" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                </Sequence>
              </Name>
              <Children>
                <Variable name="i"/>
              </Children>
            </DOMConstructor>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="avgbid" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                </Sequence>
              </Name>
              <Children>
                <Variable name="avgbid"/>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
