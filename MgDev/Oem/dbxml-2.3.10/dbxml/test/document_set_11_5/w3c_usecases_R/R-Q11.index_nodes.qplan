<XQuery>
  <FLWOR>
    <LetBinding name="highbid">
      <Function name="{http://www.w3.org/2005/xpath-functions}:max">
        <Atomize>
          <DocumentOrder>
            <DbXmlNav>
              <QueryPlanFunction result="document" container="11.5.3.dbxml">
                <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',U)</OQPlan>
              </QueryPlanFunction>
              <DbXmlStep axis="descendant" name="bid_tuple" nodeType="element"/>
              <DbXmlStep axis="child" name="bid" nodeType="element"/>
            </DbXmlNav>
          </DocumentOrder>
        </Atomize>
      </Function>
    </LetBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <FLWOR>
          <ForBinding name="item">
            <DbXmlNav>
              <QueryPlanFunction result="document" container="11.5.3.dbxml">
                <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',U)</OQPlan>
              </QueryPlanFunction>
              <DbXmlStep axis="descendant" name="item_tuple" nodeType="element"/>
            </DbXmlNav>
          </ForBinding>
          <ForBinding name="">
            <DbXmlNav>
              <LookupIndex container="11.5.3.dbxml">
                <DocumentQP index="node-metadata-equality-string" operation="eq" child="name:http://www.sleepycat.com/2002/dbxml" value="bids.xml">
                  <ValueQP index="edge-element-equality-string" operation="eq" parent="bid_tuple" child="itemno">
                    <Atomize>
                      <DbXmlNav>
                        <Variable name="item"/>
                        <DbXmlStep axis="child" name="itemno" nodeType="element">
                          <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
                        </DbXmlStep>
                      </DbXmlNav>
                    </Atomize>
                  </ValueQP>
                </DocumentQP>
              </LookupIndex>
              <Join type="parent-of-child" return="argument">
                <DbXmlNav>
                  <QueryPlanFunction result="document" container="11.5.3.dbxml">
                    <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',P(edge-element-equality-string,prefix,bid_tuple.itemno))</OQPlan>
                  </QueryPlanFunction>
                  <DbXmlStep axis="descendant" name="bid_tuple" nodeType="element"/>
                </DbXmlNav>
              </Join>
              <DbXmlFilter>
                <DbXmlCompare name="equal" join="child" name="bid" nodeType="element">
                  <Variable name="highbid"/>
                </DbXmlCompare>
              </DbXmlFilter>
            </DbXmlNav>
          </ForBinding>
          <DOMConstructor type="element">
            <Name>
              <Sequence>
                <AnyAtomicTypeConstructor value="expensive_item" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
              </Sequence>
            </Name>
            <Children>
              <DbXmlNav>
                <Variable name="item"/>
                <DbXmlStep axis="child" name="itemno" nodeType="element">
                  <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
                </DbXmlStep>
              </DbXmlNav>
              <DbXmlNav>
                <Variable name="item"/>
                <DbXmlStep axis="child" name="description" nodeType="element"/>
              </DbXmlNav>
              <DOMConstructor type="element">
                <Name>
                  <Sequence>
                    <AnyAtomicTypeConstructor value="high_bid" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                  </Sequence>
                </Name>
                <Children>
                  <Variable name="highbid"/>
                </Children>
              </DOMConstructor>
            </Children>
          </DOMConstructor>
        </FLWOR>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
