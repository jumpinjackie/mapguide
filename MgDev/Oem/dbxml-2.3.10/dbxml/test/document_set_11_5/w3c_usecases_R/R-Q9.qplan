<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <LetBinding name="end_dates">
          <DocumentOrder>
            <DbXmlNav>
              <QueryPlanFunction result="document" container="11.5.3.dbxml">
                <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',P(node-element-equality-date,prefix,end_date))</OQPlan>
              </QueryPlanFunction>
              <DbXmlStep axis="descendant" name="item_tuple" nodeType="element"/>
              <DbXmlStep axis="child" name="end_date" nodeType="element">
                <OQPlan>P(node-element-equality-date,prefix,end_date)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
          </DocumentOrder>
        </LetBinding>
        <ForBinding name="m">
          <Function name="{http://www.w3.org/2005/xpath-functions}:distinct-values">
            <FLWOR>
              <ForBinding name="e">
                <Variable name="end_dates"/>
              </ForBinding>
              <Function name="{http://www.w3.org/2005/xpath-functions}:month-from-date">
                <TreatAs>
                  <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="date">
                    <Atomize>
                      <Variable name="e"/>
                    </Atomize>
                  </PromoteUntyped>
                  <SequenceType occurrence="zero_or_one" testType="atomic-type" type="http://www.w3.org/2001/XMLSchema:date"/>
                </TreatAs>
              </Function>
            </FLWOR>
          </Function>
        </ForBinding>
        <LetBinding name="item">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',U)</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="item_tuple" nodeType="element"/>
            <DbXmlFilter>
              <Operator name="equal">
                <Function name="{http://www.w3.org/2005/xpath-functions}:year-from-date">
                  <TreatAs>
                    <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="date">
                      <Atomize>
                        <DbXmlStep axis="child" name="end_date" nodeType="element">
                          <OQPlan>P(node-element-equality-date,prefix,end_date)</OQPlan>
                        </DbXmlStep>
                      </Atomize>
                    </PromoteUntyped>
                    <SequenceType occurrence="zero_or_one" testType="atomic-type" type="http://www.w3.org/2001/XMLSchema:date"/>
                  </TreatAs>
                </Function>
                <Sequence>
                  <AnyAtomicTypeConstructor value="1999" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                </Sequence>
              </Operator>
            </DbXmlFilter>
            <DbXmlFilter>
              <Operator name="equal">
                <Function name="{http://www.w3.org/2005/xpath-functions}:month-from-date">
                  <TreatAs>
                    <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="date">
                      <Atomize>
                        <DbXmlStep axis="child" name="end_date" nodeType="element">
                          <OQPlan>P(node-element-equality-date,prefix,end_date)</OQPlan>
                        </DbXmlStep>
                      </Atomize>
                    </PromoteUntyped>
                    <SequenceType occurrence="zero_or_one" testType="atomic-type" type="http://www.w3.org/2001/XMLSchema:date"/>
                  </TreatAs>
                </Function>
                <Variable name="m"/>
              </Operator>
            </DbXmlFilter>
          </DbXmlNav>
        </LetBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <TreatAs>
              <Variable name="m"/>
              <SequenceType occurrence="zero_or_one" testType="anything"/>
            </TreatAs>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="monthly_result" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="month" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                </Sequence>
              </Name>
              <Children>
                <Variable name="m"/>
              </Children>
            </DOMConstructor>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="item_count" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                </Sequence>
              </Name>
              <Children>
                <Function name="{http://www.w3.org/2005/xpath-functions}:count">
                  <Variable name="item"/>
                </Function>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
