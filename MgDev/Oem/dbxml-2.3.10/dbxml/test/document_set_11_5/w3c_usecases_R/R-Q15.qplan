<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="u">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'users.xml',U)</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="user_tuple" nodeType="element"/>
          </DbXmlNav>
        </ForBinding>
        <LetBinding name="b">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(edge-element-equality-string,bid_tuple.userid,=,'[to be calculated]'))</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="bid_tuple" nodeType="element"/>
            <DbXmlFilter>
              <DbXmlCompare name="equal" join="child" name="userid" nodeType="element">
                <OQPlan>V(edge-element-equality-string,bid_tuple.userid,=,'[to be calculated]')</OQPlan>
                <Atomize>
                  <DbXmlNav>
                    <Variable name="u"/>
                    <DbXmlStep axis="child" name="userid" nodeType="element">
                      <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
                    </DbXmlStep>
                  </DbXmlNav>
                </Atomize>
              </DbXmlCompare>
            </DbXmlFilter>
            <DbXmlFilter>
              <DbXmlCompare name="greater_than_equal" join="child" name="bid" nodeType="element">
                <Sequence>
                  <AnyAtomicTypeConstructor value="100" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                </Sequence>
              </DbXmlCompare>
            </DbXmlFilter>
          </DbXmlNav>
          <Where>
            <Operator name="greater_than">
              <Function name="{http://www.w3.org/2005/xpath-functions}:count">
                <Variable name="b"/>
              </Function>
              <Sequence>
                <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
              </Sequence>
            </Operator>
          </Where>
        </LetBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="big_spender" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DocumentOrder>
              <DbXmlNav>
                <Variable name="u"/>
                <DbXmlStep axis="child" name="name" nodeType="element"/>
                <DbXmlStep axis="child">
                  <ItemType testType="text"/>
                </DbXmlStep>
              </DbXmlNav>
            </DocumentOrder>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
