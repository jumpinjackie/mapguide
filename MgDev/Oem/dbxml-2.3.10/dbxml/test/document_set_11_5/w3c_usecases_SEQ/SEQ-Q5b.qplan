<XQuery>
  <FunctionDefinition name="{http://www.w3.org/2005/xquery-local-functions}:between">
    <FLWOR>
      <LetBinding name="nodes">
        <FLWOR>
          <ForBinding name="n">
            <DocumentOrder>
              <Operator name="except">
                <Variable name="seq"/>
                <DocumentOrder>
                  <DbXmlNav>
                    <Variable name="start"/>
                    <DbXmlStep axis="descendant">
                      <ItemType testType="node"/>
                    </DbXmlStep>
                  </DbXmlNav>
                </DocumentOrder>
              </Operator>
            </DocumentOrder>
            <Where>
              <Operator name="and">
                <Operator name="node-order">
                  <TreatAs>
                    <Variable name="n"/>
                    <SequenceType occurrence="zero_or_one" testType="node"/>
                  </TreatAs>
                  <TreatAs>
                    <Variable name="start"/>
                    <SequenceType occurrence="zero_or_one" testType="node"/>
                  </TreatAs>
                </Operator>
                <Operator name="node-order">
                  <TreatAs>
                    <Variable name="n"/>
                    <SequenceType occurrence="zero_or_one" testType="node"/>
                  </TreatAs>
                  <TreatAs>
                    <Variable name="end"/>
                    <SequenceType occurrence="zero_or_one" testType="node"/>
                  </TreatAs>
                </Operator>
              </Operator>
            </Where>
          </ForBinding>
          <Variable name="n"/>
        </FLWOR>
      </LetBinding>
      <Operator name="except">
        <Variable name="nodes"/>
        <DbXmlNav>
          <Variable name="nodes"/>
          <DbXmlStep axis="descendant">
            <ItemType testType="node"/>
          </DbXmlStep>
        </DbXmlNav>
      </Operator>
    </FLWOR>
  </FunctionDefinition>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="critical_sequence" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <LetBinding name="proc">
          <DocumentOrder>
            <DbXmlNav>
              <QueryPlanFunction result="document" container="11.5.4.dbxml">
                <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'report1.xml',V(edge-element-equality-string,section.section.title,=,'Procedure'))</OQPlan>
              </QueryPlanFunction>
              <DbXmlStep axis="descendant-or-self" uri="*" name="*" nodeType="*"/>
              <Predicate>
                <DbXmlNav>
                  <DbXmlStep axis="child" name="section" nodeType="element"/>
                  <DbXmlFilter>
                    <DbXmlCompare name="equal" join="child" name="section.title" nodeType="element">
                      <OQPlan>V(edge-element-equality-string,section.section.title,=,'Procedure')</OQPlan>
                      <Sequence>
                        <AnyAtomicTypeConstructor value="Procedure" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                      </Sequence>
                    </DbXmlCompare>
                  </DbXmlFilter>
                </DbXmlNav>
                <Sequence>
                  <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                </Sequence>
              </Predicate>
            </DbXmlNav>
          </DocumentOrder>
        </LetBinding>
        <LetBinding name="first">
          <Predicate>
            <DocumentOrder>
              <DbXmlNav>
                <Variable name="proc"/>
                <DbXmlStep axis="descendant" name="incision" nodeType="element">
                  <OQPlan>P(node-element-presence-none,=,incision)</OQPlan>
                </DbXmlStep>
              </DbXmlNav>
            </DocumentOrder>
            <Sequence>
              <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
            </Sequence>
          </Predicate>
        </LetBinding>
        <LetBinding name="second">
          <Predicate>
            <DocumentOrder>
              <DbXmlNav>
                <Variable name="proc"/>
                <DbXmlStep axis="descendant" name="incision" nodeType="element">
                  <OQPlan>P(node-element-presence-none,=,incision)</OQPlan>
                </DbXmlStep>
              </DbXmlNav>
            </DocumentOrder>
            <Sequence>
              <AnyAtomicTypeConstructor value="2" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
            </Sequence>
          </Predicate>
        </LetBinding>
        <UserFunction name="{http://www.w3.org/2005/xquery-local-functions}:between">
          <Binding name="{}:seq">
            <DocumentOrder>
              <DbXmlNav>
                <Variable name="proc"/>
                <DbXmlStep axis="descendant">
                  <ItemType testType="node"/>
                </DbXmlStep>
              </DbXmlNav>
            </DocumentOrder>
          </Binding>
          <Binding name="{}:start">
            <TreatAs>
              <Variable name="first"/>
              <SequenceType occurrence="exactly_one" testType="node"/>
            </TreatAs>
          </Binding>
          <Binding name="{}:end">
            <TreatAs>
              <Variable name="second"/>
              <SequenceType occurrence="exactly_one" testType="node"/>
            </TreatAs>
          </Binding>
        </UserFunction>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
