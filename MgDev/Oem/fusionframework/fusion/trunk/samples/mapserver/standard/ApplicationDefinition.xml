  <MapSet xsi:type="MapSetType">
    <MapGroup id="mapguide" xsi:type="MapType">
        <InitialView xsi:type="MapViewType">
            <CenterX>1</CenterX>
            <CenterY>1</CenterY>
            <Scale>1</Scale>
        </InitialView>
      <Map xsi:type="MapGuideLayerType">
        <Type>MapGuide</Type>
        <SingleTile>true</SingleTile>
        <ResourceId>Library://Samples/Sheboyan/Maps/Sheboygan.MapDefinition</ResourceId>
      </Map>
    </MapGroup>
    <MapGroup id="mapserver" xsi:type="MapType">
         <!-- example should be centered on Ottawa -->
        <!--InitialView xsi:type="MapViewType">
            <CenterX>1500000</CenterX> 
            <CenterY>-161000</CenterY>
            <Scale>3000000</Scale>
        </InitialView-->
      <Map xsi:type="MapLayerType">
        <Type>MapServer</Type>
        <SingleTile>true</SingleTile>
        <Extension>
            <MapFile>/ms4w/apps/gmap/htdocs/gmap75.map</MapFile>
        </Extension>
      </Map>
    </MapGroup>
    <MapGroup id="mapserverKeymap" xsi:type="MapType">
      <Map xsi:type="MapLayerType">
        <Type>MapServer</Type>
        <SingleTile>true</SingleTile>
        <Extension>
