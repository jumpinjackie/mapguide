<?xml version="1.0" encoding="UTF-8"?>
<WebLayout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="WebLayout-Fusion-0.1.xsd">
    <CommandSet>
        <Command xsi:type="MapCommandType">
            <Name>Map</Name>
            <Action>MSMap</Action>
            <MapFile>/ms4w/apps/gmap/htdocs/gmap75.map</MapFile>
        </Command>
        <Command>
            <Name>ZoomIn</Name>
            <ImageURL>images/icons/zoom-in.png</ImageURL>
            <Action>Zoom</Action>
        </Command>
        <Command>
            <Name>ZoomOut</Name>
            <ImageURL>images/icons/zoom-out.png</ImageURL>
            <Action>ZoomOut</Action>
        </Command>
        <Command>
            <Name>Pan</Name>
            <ImageURL>images/icons/pan.png</ImageURL>
            <Action>Pan</Action>
        </Command>
        <Command>
            <Name>FitToWindow</Name>
            <ImageURL>images/icons/zoom-full.png</ImageURL>
            <Action>FitToWindow</Action>
        </Command>
    </CommandSet>
</WebLayout>