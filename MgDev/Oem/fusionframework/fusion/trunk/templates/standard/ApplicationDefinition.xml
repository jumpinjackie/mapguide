  <TemplateUrl>fusion/templates/standard/index.html</TemplateUrl>
  <MapSet>
    <MapGroup id="mapguide" xsi:type="MapType">
      <!-- should be centered on intersection of Lower Falls Rd and S.Taylor Dr. -->
      <InitialView xsi:type="MapViewType">
          <CenterX>-87.745061</CenterX>
          <CenterY>43.740283</CenterY>
          <Scale>8000</Scale>
      </InitialView>
      <Map xsi:type="MapGuideLayerType">
        <Type>MapGuide</Type>
        <SingleTile>true</SingleTile>
        <ResourceId>Library://Samples/Sheboygan/Maps/Sheboygan.MapDefinition</ResourceId>
      </Map>
    </MapGroup>
    <MapGroup id="MgMainMapId">
      <Map>
        <Type>MapGuide</Type>
        <Extension>
          <ResourceId>Library://Samples/Sheboygan/Maps/Sheboygan.MapDefinition</ResourceId>
        </Extension>
      </Map>
      <Extension />
    </MapGroup>
  </MapSet>
