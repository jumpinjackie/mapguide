      <Name>ColorPicker</Name>
      <Type>ColorPicker</Type>
      <Extension />
    </Widget>
    <Widget>
      <Name>CursorPosition</Name>
      <Type>CursorPosition</Type>
      <Extension>
        <Precision>4</Precision>
        <Template>x: {x}, y: {y}</Template>
        <Units>dd</Units>
      </Extension>
    </Widget>
    <Widget xsi:type="UiWidgetType">
