<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <Function name="{http://www.w3.org/2005/04/xpath-functions}:distinct-values">
        <Navigation>
          <QueryPlanFunction result="collection" container="11.8.1.dbxml">
            <OQPlan>n(P(node-element-equality-string,prefix,customer_id),V(node-element-equality-decimal,total,>,'11000'),P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order))</OQPlan>
          </QueryPlanFunction>
          <Step axis="child" name="order" nodeType="element">
            <OQPlan>P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order)</OQPlan>
          </Step>
          <DbXmlFilter>
            <Navigation>
              <Step axis="child" name="total" nodeType="element">
                <OQPlan>V(node-element-equality-decimal,total,>,'11000')</OQPlan>
              </Step>
              <DbXmlCompare name="greater_than">
                <Sequence>
                  <AnyAtomicTypeConstructor value="11000.0" typeuri="http://www.w3.org/2001/XMLSchema" typename="decimal"/>
                </Sequence>
              </DbXmlCompare>
            </Navigation>
          </DbXmlFilter>
          <Step axis="child" name="customer_id" nodeType="element">
            <OQPlan>P(node-element-equality-string,prefix,customer_id)</OQPlan>
          </Step>
        </Navigation>
      </Function>
    </ForBinding>
    <LetBinding name="b">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.1.dbxml">
          <RQPlan>n(P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order),V(customer_id,=,'[to be calculated]'))</RQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="order" nodeType="element">
          <OQPlan>P(edge-element-presence-none,=,root:http://www.sleepycat.com/2002/dbxml.order)</OQPlan>
        </Step>
        <DbXmlFilter>
          <Navigation>
            <Step axis="child" name="customer_id" nodeType="element">
              <RQPlan>V(customer_id,=,'[to be calculated]')</RQPlan>
            </Step>
            <DbXmlCompare name="equal">
              <Variable name="a"/>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </LetBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="CustKey" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Variable name="a"/>
          </Children>
        </DOMConstructor>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="NumberOfOrders" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Function name="{http://www.w3.org/2005/04/xpath-functions}:count">
              <Variable name="b"/>
            </Function>
          </Children>
