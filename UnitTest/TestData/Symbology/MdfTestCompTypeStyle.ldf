                        <LineCap>'None'</LineCap>
                        <LineJoin>'None'</LineJoin>
                        <LineMiterLimit>5.0</LineMiterLimit>
                      </Path>
                      <Image>
                        <ResizeControl>'AddToResizeBox'</ResizeControl>
                        <Content>R0lGODlhcgGSALMAAAQCAEMmCZtuMFQxDS8b</Content>
