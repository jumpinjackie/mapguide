                        <Geometry>M 0,0 L 5,0 L 5,5 L 0,5 L 0,0</Geometry>
                        <FillColor>ffff0000</FillColor>
                        <LineColor>%OUTLINE%</LineColor>
                        <LineWeight>2*(%THICKNESS% + 1)</LineWeight>
