          <DbXmlNav>
            <Variable name="a"/>
            <DbXmlStep axis="descendant">
              <ItemType testType="node"/>
            </DbXmlStep>
            <Join type="self" return="context_item">
              <Variable name="b"/>
            </Join>
          </DbXmlNav>
        </Function>
      </Operator>
      <SequenceType occurrence="exactly_one" testType="atomic-type" type="http://www.w3.org/2001/XMLSchema:boolean"/>
