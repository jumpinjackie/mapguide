              </QueryPlanFunction>
              <DbXmlStep axis="descendant-or-self" uri="*" name="*" nodeType="*"/>
              <Predicate>
                <DbXmlNav>
                  <LookupIndex container="11.5.4.dbxml">
                    <ValueQP index="edge-element-equality-string" operation="eq" parent="section" child="section.title" value="Procedure"/>
                  </LookupIndex>
                  <Join type="parent-of-child" return="argument">
