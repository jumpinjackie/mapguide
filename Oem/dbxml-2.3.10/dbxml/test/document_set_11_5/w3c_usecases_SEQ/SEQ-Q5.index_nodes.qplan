              <DbXmlStep axis="descendant">
                <ItemType testType="node"/>
              </DbXmlStep>
            </DbXmlNav>
          </DocumentOrder>
          <Where>
            <Operator name="and">
              <UserFunction name="{http://www.w3.org/2005/xquery-local-functions}:follows">
                <Binding name="{}:a">
                  <TreatAs>
                    <Variable name="n"/>
                    <SequenceType occurrence="exactly_one" testType="node"/>
                  </TreatAs>
                </Binding>
                <Binding name="{}:b">
                  <TreatAs>
                    <Predicate>
                      <DocumentOrder>
                        <DbXmlNav>
                          <Variable name="proc"/>
                          <DbXmlStep axis="descendant" name="incision" nodeType="element">
                            <OQPlan>P(node-element-presence-none,=,incision)</OQPlan>
                          </DbXmlStep>
                        </DbXmlNav>
                      </DocumentOrder>
                      <Sequence>
                        <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                      </Sequence>
                    </Predicate>
                    <SequenceType occurrence="exactly_one" testType="node"/>
                  </TreatAs>
                </Binding>
              </UserFunction>
              <UserFunction name="{http://www.w3.org/2005/xquery-local-functions}:precedes">
                <Binding name="{}:a">
                  <TreatAs>
                    <Variable name="n"/>
                    <SequenceType occurrence="exactly_one" testType="node"/>
                  </TreatAs>
                </Binding>
                <Binding name="{}:b">
                  <TreatAs>
                    <Predicate>
                      <DocumentOrder>
                        <DbXmlNav>
                          <Variable name="proc"/>
                          <DbXmlStep axis="descendant" name="incision" nodeType="element">
                            <OQPlan>P(node-element-presence-none,=,incision)</OQPlan>
                          </DbXmlStep>
