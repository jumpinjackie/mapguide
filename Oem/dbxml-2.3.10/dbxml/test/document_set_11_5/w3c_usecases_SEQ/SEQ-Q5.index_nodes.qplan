<XQuery>
  <FunctionDefinition name="{http://www.w3.org/2005/xquery-local-functions}:precedes">
    <TreatAs>
      <Operator name="and">
        <Operator name="node-order">
          <TreatAs>
            <Variable name="a"/>
            <SequenceType occurrence="zero_or_one" testType="node"/>
          </TreatAs>
          <TreatAs>
            <Variable name="b"/>
            <SequenceType occurrence="zero_or_one" testType="node"/>
