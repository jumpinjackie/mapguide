<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="bib" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="b">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.8.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bib.xml',U)</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="book" nodeType="element"/>
          </DbXmlNav>
          <Where>
            <Operator name="greater_than">
              <Function name="{http://www.w3.org/2005/xpath-functions}:count">
                <DbXmlNav>
                  <Variable name="b"/>
                  <DbXmlStep axis="child" name="author" nodeType="element"/>
                </DbXmlNav>
              </Function>
              <Sequence>
                <AnyAtomicTypeConstructor value="0" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
              </Sequence>
            </Operator>
          </Where>
        </ForBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="book" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DbXmlNav>
              <Variable name="b"/>
              <DbXmlStep axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
            <DbXmlNav>
              <Variable name="b"/>
              <Predicate>
                <DbXmlStep axis="child" name="author" nodeType="element"/>
                <Operator name="less_than_equal">
                  <Function name="{http://www.w3.org/2005/xpath-functions}:position"/>
                  <Sequence>
                    <AnyAtomicTypeConstructor value="2" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                  </Sequence>
                </Operator>
              </Predicate>
            </DbXmlNav>
            <If>
              <Test>
                <Operator name="greater_than">
                  <Function name="{http://www.w3.org/2005/xpath-functions}:count">
                    <DbXmlNav>
                      <Variable name="b"/>
                      <DbXmlStep axis="child" name="author" nodeType="element"/>
                    </DbXmlNav>
                  </Function>
                  <Sequence>
                    <AnyAtomicTypeConstructor value="2" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                  </Sequence>
                </Operator>
              </Test>
              <Then>
                <DOMConstructor type="element">
                  <Name>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="et-al" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                    </Sequence>
                  </Name>
                </DOMConstructor>
              </Then>
              <Else>
                <Sequence/>
              </Else>
            </If>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
