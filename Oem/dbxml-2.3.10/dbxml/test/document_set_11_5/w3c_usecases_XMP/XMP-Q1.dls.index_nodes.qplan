<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="bib" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="b">
          <DbXmlNav>
            <LookupIndex container="11.5.8.dbxml">
              <DocumentQP index="node-metadata-equality-string" operation="eq" child="name:http://www.sleepycat.com/2002/dbxml" value="bib.xml">
                <ValueQP index="edge-attribute-equality-double" operation="gt" parent="book" child="year" value="1991"/>
              </DocumentQP>
            </LookupIndex>
            <Join type="parent-of-attribute" return="argument">
              <DbXmlNav>
                <QueryPlanFunction result="document" container="11.5.8.dbxml">
                  <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bib.xml',n(V(edge-element-equality-string,book.publisher,=,'Addison-Wesley'),V(edge-attribute-equality-double,book.@year,>,'1991')))</OQPlan>
                </QueryPlanFunction>
                <DbXmlStep axis="child" name="bib" nodeType="element"/>
                <DbXmlStep axis="child" name="book" nodeType="element"/>
                <Join type="child" return="context_item">
                  <LookupIndex container="11.5.8.dbxml">
                    <ValueQP index="edge-element-equality-string" operation="eq" parent="book" child="publisher" value="Addison-Wesley"/>
                  </LookupIndex>
                </Join>
              </DbXmlNav>
            </Join>
          </DbXmlNav>
        </ForBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="book" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Attributes>
            <DOMConstructor type="attribute">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="year" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                </Sequence>
              </Name>
              <Children>
                <Atomize>
                  <DbXmlNav>
                    <Variable name="b"/>
                    <DbXmlStep axis="attribute" name="year" nodeType="attribute">
                      <OQPlan>P(edge-attribute-equality-double,prefix,book.@year)</OQPlan>
                    </DbXmlStep>
                  </DbXmlNav>
                </Atomize>
              </Children>
            </DOMConstructor>
          </Attributes>
          <Children>
            <DbXmlNav>
              <Variable name="b"/>
              <DbXmlStep axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
