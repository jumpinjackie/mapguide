<XQuery>
  <FLWOR>
    <LetBinding name="item">
      <DbXmlNav>
        <QueryPlanFunction result="document" container="11.5.3.dbxml">
          <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',R(node-element-equality-date,end_date,>=,'1999-03-01',<=,'1999-03-31'))</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="descendant" name="item_tuple" nodeType="element"/>
        <DbXmlFilter>
          <DbXmlCompare name="greater_than_equal" join="child" name="end_date" nodeType="element">
            <OQPlan>V(node-element-equality-date,end_date,>=,'1999-03-01')</OQPlan>
            <Sequence>
              <AnyAtomicTypeConstructor value="1999-03-01" typeuri="http://www.w3.org/2001/XMLSchema" typename="date"/>
            </Sequence>
          </DbXmlCompare>
        </DbXmlFilter>
        <DbXmlFilter>
          <DbXmlCompare name="less_than_equal" join="child" name="end_date" nodeType="element">
            <OQPlan>V(node-element-equality-date,end_date,<=,'1999-03-31')</OQPlan>
            <Sequence>
              <AnyAtomicTypeConstructor value="1999-03-31" typeuri="http://www.w3.org/2001/XMLSchema" typename="date"/>
            </Sequence>
          </DbXmlCompare>
        </DbXmlFilter>
      </DbXmlNav>
    </LetBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="item_count" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <Function name="{http://www.w3.org/2005/xpath-functions}:count">
          <Variable name="item"/>
        </Function>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
