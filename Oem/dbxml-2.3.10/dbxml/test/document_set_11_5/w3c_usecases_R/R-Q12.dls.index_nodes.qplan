<XQuery>
  <FunctionDefinition name="{http://www.w3.org/2005/xquery-local-functions}:bid_summary">
    <FLWOR>
      <ForBinding name="i">
        <Function name="{http://www.w3.org/2005/xpath-functions}:distinct-values">
          <Atomize>
            <LookupIndex container="11.5.3.dbxml">
              <DocumentQP index="node-metadata-equality-string" operation="eq" child="name:http://www.sleepycat.com/2002/dbxml" value="bids.xml">
                <PresenceQP index="edge-element-equality-string" operation="prefix" child="itemno"/>
              </DocumentQP>
            </LookupIndex>
          </Atomize>
        </Function>
      </ForBinding>
      <LetBinding name="b">
        <DbXmlNav>
          <LookupIndex container="11.5.3.dbxml">
            <DocumentQP index="node-metadata-equality-string" operation="eq" child="name:http://www.sleepycat.com/2002/dbxml" value="bids.xml">
              <ValueQP index="edge-element-equality-string" operation="eq" parent="bid_tuple" child="itemno">
                <Variable name="i"/>
              </ValueQP>
            </DocumentQP>
          </LookupIndex>
          <Join type="parent-of-child" return="argument">
            <DbXmlNav>
              <QueryPlanFunction result="document" container="11.5.3.dbxml">
                <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',P(edge-element-equality-string,prefix,bid_tuple.itemno))</OQPlan>
              </QueryPlanFunction>
              <DbXmlStep axis="descendant" name="bid_tuple" nodeType="element"/>
            </DbXmlNav>
          </Join>
        </DbXmlNav>
      </LetBinding>
      <DOMConstructor type="element">
        <Name>
          <Sequence>
            <AnyAtomicTypeConstructor value="bid_count" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
          </Sequence>
        </Name>
        <Children>
          <DOMConstructor type="element">
            <Name>
              <Sequence>
                <AnyAtomicTypeConstructor value="itemno" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
              </Sequence>
            </Name>
            <Children>
              <Variable name="i"/>
            </Children>
          </DOMConstructor>
          <DOMConstructor type="element">
            <Name>
              <Sequence>
                <AnyAtomicTypeConstructor value="nbids" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
              </Sequence>
            </Name>
            <Children>
              <Function name="{http://www.w3.org/2005/xpath-functions}:count">
                <Variable name="b"/>
              </Function>
            </Children>
          </DOMConstructor>
        </Children>
      </DOMConstructor>
    </FLWOR>
  </FunctionDefinition>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <LetBinding name="bid_counts">
          <UserFunction name="{http://www.w3.org/2005/xquery-local-functions}:bid_summary">
          </UserFunction>
        </LetBinding>
        <LetBinding name="maxbids">
          <Function name="{http://www.w3.org/2005/xpath-functions}:max">
            <Atomize>
              <DocumentOrder>
                <DbXmlNav>
                  <Variable name="bid_counts"/>
                  <DbXmlStep axis="child" name="nbids" nodeType="element"/>
                </DbXmlNav>
              </DocumentOrder>
            </Atomize>
          </Function>
        </LetBinding>
        <ForBinding name="item">
          <DbXmlNav>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',U)</OQPlan>
            </QueryPlanFunction>
            <DbXmlStep axis="descendant" name="item_tuple" nodeType="element"/>
          </DbXmlNav>
        </ForBinding>
        <ForBinding name="bc">
          <DocumentOrder>
            <DbXmlNav>
              <Variable name="bid_counts"/>
              <DbXmlFilter>
                <DbXmlCompare name="equal" join="child" name="nbids" nodeType="element">
                  <Variable name="maxbids"/>
                </DbXmlCompare>
              </DbXmlFilter>
            </DbXmlNav>
          </DocumentOrder>
          <Where>
            <DbXmlNav>
              <Variable name="item"/>
              <DbXmlCompare name="equal" join="child" name="itemno" nodeType="element">
                <OQPlan>V(edge-element-equality-string,item_tuple.itemno,=,'[to be calculated]')</OQPlan>
                <Atomize>
                  <DbXmlNav>
                    <Variable name="bc"/>
                    <DbXmlStep axis="child" name="itemno" nodeType="element"/>
                  </DbXmlNav>
                </Atomize>
              </DbXmlCompare>
            </DbXmlNav>
          </Where>
        </ForBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="popular_item" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
            </Sequence>
          </Name>
          <Children>
            <DbXmlNav>
              <Variable name="item"/>
              <DbXmlStep axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
              </DbXmlStep>
            </DbXmlNav>
            <DbXmlNav>
              <Variable name="item"/>
              <DbXmlStep axis="child" name="description" nodeType="element"/>
            </DbXmlNav>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="bid_count" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
                </Sequence>
              </Name>
              <Children>
                <DocumentOrder>
                  <DbXmlNav>
                    <Variable name="bc"/>
                    <DbXmlStep axis="child" name="nbids" nodeType="element"/>
                    <DbXmlStep axis="child">
                      <ItemType testType="text"/>
                    </DbXmlStep>
                  </DbXmlNav>
                </DocumentOrder>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
