<XQuery>
  <FLWOR>
    <ForBinding name="size">
      <DbXmlNav>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>U</OQPlan>
        </QueryPlanFunction>
        <DbXmlStep axis="child" name="catalog" nodeType="element"/>
        <DbXmlStep axis="child" name="item" nodeType="element"/>
        <DbXmlStep axis="child" name="attributes" nodeType="element"/>
        <DbXmlStep axis="child" name="size_of_book" nodeType="element"/>
      </DbXmlNav>
      <Where>
        <Operator name="greater_than">
          <Operator name="mul">
            <TreatAs>
              <Operator name="mul">
                <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="double">
                  <TreatAs>
                    <Atomize>
                      <DbXmlNav>
                        <Variable name="size"/>
                        <DbXmlStep axis="child" name="length" nodeType="element"/>
                      </DbXmlNav>
                    </Atomize>
                    <SequenceType occurrence="zero_or_one" testType="anything"/>
                  </TreatAs>
                </PromoteUntyped>
                <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="double">
                  <TreatAs>
                    <Atomize>
                      <DbXmlNav>
                        <Variable name="size"/>
                        <DbXmlStep axis="child" name="width" nodeType="element"/>
                      </DbXmlNav>
                    </Atomize>
                    <SequenceType occurrence="zero_or_one" testType="anything"/>
                  </TreatAs>
                </PromoteUntyped>
              </Operator>
              <SequenceType occurrence="zero_or_one" testType="anything"/>
            </TreatAs>
            <PromoteUntyped uri="http://www.w3.org/2001/XMLSchema" name="double">
              <TreatAs>
                <Atomize>
                  <DbXmlNav>
                    <Variable name="size"/>
                    <DbXmlStep axis="child" name="height" nodeType="element"/>
                  </DbXmlNav>
                </Atomize>
                <SequenceType occurrence="zero_or_one" testType="anything"/>
              </TreatAs>
            </PromoteUntyped>
          </Operator>
          <Sequence>
            <AnyAtomicTypeConstructor value="500000" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
          </Sequence>
        </Operator>
      </Where>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="QName"/>
        </Sequence>
      </Name>
      <Children>
        <DbXmlNav>
          <Variable name="size"/>
          <DbXmlStep axis="parent" uri="*" name="*" nodeType="*"/>
          <DbXmlStep axis="parent" uri="*" name="*" nodeType="*"/>
          <DbXmlStep axis="child" name="title" nodeType="element"/>
        </DbXmlNav>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
