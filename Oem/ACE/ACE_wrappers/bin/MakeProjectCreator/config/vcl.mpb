project {
  requires += vcl
  macros   += ACE_HAS_VCL=1
  specific(bmake, bds4) {
     use_vcl        = 1
     lit_libs      += rtl vcl
     compile_flags += -tW -tWV
     linkflags     += -aa
  }
  verbatim(borland,macros) {
    VCL=1
  }
}
