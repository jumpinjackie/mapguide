echo "$as_me: WARNING: We do not know if rtti needs enabling for this compiler." >&2;}
      fi
      ;;
    no)
      ace_user_enable_rtti=no
      ;;
    *)
      { { echo "$as_me:$LINENO: error: bad value ${enableval} for --enable-rtti" >&5
echo "$as_me: error: bad value ${enableval} for --enable-rtti" >&2;}
   { (exit 1); exit 1; }; }
      ;;
   esac

fi;

