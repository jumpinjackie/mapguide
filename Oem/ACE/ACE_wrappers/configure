cat >>$CONFIG_STATUS <<\_ACEOF
for ac_config_target in $ac_config_targets
do
  case "$ac_config_target" in
  # Handling of arguments.
  "ace-config" ) CONFIG_FILES="$CONFIG_FILES ace-config" ;;
  "ace-config.1" ) CONFIG_FILES="$CONFIG_FILES ace-config.1" ;;
  "aceConf.sh" ) CONFIG_FILES="$CONFIG_FILES aceConf.sh" ;;
  "Makefile" ) CONFIG_FILES="$CONFIG_FILES Makefile" ;;
  "ace/Makefile" ) CONFIG_FILES="$CONFIG_FILES ace/Makefile" ;;
  "ace/RMCast/Makefile" ) CONFIG_FILES="$CONFIG_FILES ace/RMCast/Makefile" ;;
  "ace/QoS/Makefile" ) CONFIG_FILES="$CONFIG_FILES ace/QoS/Makefile" ;;
  "ace/SSL/Makefile" ) CONFIG_FILES="$CONFIG_FILES ace/SSL/Makefile" ;;
  "apps/Makefile" ) CONFIG_FILES="$CONFIG_FILES apps/Makefile" ;;
  "apps/gperf/Makefile" ) CONFIG_FILES="$CONFIG_FILES apps/gperf/Makefile" ;;
  "apps/gperf/src/Makefile" ) CONFIG_FILES="$CONFIG_FILES apps/gperf/src/Makefile" ;;
