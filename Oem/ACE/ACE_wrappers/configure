      enable_shared_with_static_runtimes_F77=yes
      ;;

    darwin* | rhapsody*)
    if $CC -v 2>&1 | grep 'Apple' >/dev/null ; then
      archive_cmds_need_lc_F77=no
      case "$host_os" in
      rhapsody* | darwin1.[012])
	allow_undefined_flag_F77='-undefined suppress'
	;;
      *) # Darwin 1.3 on
