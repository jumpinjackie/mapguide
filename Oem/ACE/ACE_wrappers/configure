  s = sem_open ("ace_semaphore_foo", O_CREAT | O_EXCL, 0600, 1);

  if (s == SEM_FAILED)
    return -1;  /* FAILURE */

  if (sem_close (s) != 0)
    return -1;  /* Something went wrong! */

  return 0;
}

