        PRELINK="$QUANTIFY -best-effort -max_threads=100 -cache-dir=$PURE_CACHE_DIR"
        		    ACE_QUANTIFY_DIR=`type quantify | sed -e 's/.* is //' -e 's%/quantify$$%%'`
        ACE_CPPFLAGS="-DACE_HAS_QUANTIFY -I$ACE_QUANTIFY_DIR"
      else
        { echo "$as_me:$LINENO: WARNING: Quantify program was not found." >&5
echo "$as_me: WARNING: Quantify program was not found." >&2;}
        { echo "$as_me:$LINENO: WARNING: Disabling quantify support." >&5
echo "$as_me: WARNING: Disabling quantify support." >&2;}
      fi
      ;;
    no)
      PRELINK=""
      ;;
    *)
      { { echo "$as_me:$LINENO: error: bad value ${enableval} for --enable-quantify" >&5
echo "$as_me: error: bad value ${enableval} for --enable-quantify" >&2;}
   { (exit 1); exit 1; }; }
      ;;
   esac

else
  PRELINK=""
fi;

 # Check whether --enable-repo or --disable-repo was given.
if test "${enable_repo+set}" = set; then
  enableval="$enable_repo"

   case "${enableval}" in
    yes)
      if test "$GXX" = yes; then
        ace_user_enable_repo=yes
        ACE_CXXFLAGS="$ACE_CXXFLAGS -frepo"
        cat >>confdefs.h <<\_ACEOF
#define ACE_HAS_GNU_REPO 1
_ACEOF

      else
        ace_user_enable_repo=no
        { echo "$as_me:$LINENO: WARNING: Not using GNU C++! GNU template respository disabled." >&5
echo "$as_me: WARNING: Not using GNU C++! GNU template respository disabled." >&2;}
      fi
      ;;
    no)
      ace_user_enable_repo=no
      ;;
    *)
      { { echo "$as_me:$LINENO: error: bad value ${enableval} for --enable-repo" >&5
echo "$as_me: error: bad value ${enableval} for --enable-repo" >&2;}
   { (exit 1); exit 1; }; }
      ;;
   esac

else

   ace_user_enable_repo=no

fi;

 # Check whether --enable-rtti or --disable-rtti was given.
if test "${enable_rtti+set}" = set; then
