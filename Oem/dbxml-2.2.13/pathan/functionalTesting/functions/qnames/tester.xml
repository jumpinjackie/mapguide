<?xml version="1.0" encoding="utf-8"?>
<test purpose="functions">
  <policies>
    <policy name="QNames">
      <addArg>-N</addArg>
      <addArg>def</addArg>
      <addArg>http://defaultns.org/</addArg>
      <addArg>-N</addArg>
      <addArg>to</addArg>
      <addArg>http://dddd</addArg>
      <addArg>test.xml</addArg>
    </policy>
    <default>
      <program>../../../samples/evaluator/eval</program>
    </default>
  </policies>
  <base type="pass">
    <arg>resolve-QName(&quot;fred&quot;, /testcase)</arg>
    <output>:fred
</output>
  </base>
  <base type="pass">
    <arg>resolve-QName(&quot;ha:fred&quot;,  /testcase)</arg>
    <output>no namespace found for prefix
</output>
  </base>
  <base type="pass">
    <arg>resolve-QName(&quot;ha:fred&quot;, /testcase/def:namespace-stuff/*[2])</arg>
    <output>http://sluggy.com:fred
</output>
  </base>
  <base type="pass">
    <arg>resolve-QName(&quot;hello&quot;,  /testcase)</arg>
    <output>:hello
</output>
  </base>
  <base type="pass">
    <arg>resolve-QName(&quot;barney&quot;, /testcase/def:namespace-stuff/*[1])</arg>
    <output>http://defaultns.org/:barney
</output>
  </base>
  <base type="pass">
    <arg>namespace-uri-from-QName(resolve-QName(&quot;hee:fred&quot;, /testcase))</arg>
    <output>http://www.foobar.com
</output>
  </base>
  <base type="pass">
    <arg>local-name-from-QName(resolve-QName(&quot;hee:fred&quot;, /testcase))</arg>
    <output>fred
</output>
  </base>
  <base type="pass">
    <arg>local-name-from-QName(())</arg>
    <output/>
  </base>
  <base type="pass">
    <arg>namespace-uri-from-QName(())</arg>
    <output/>
  </base>
  <base type="pass">
    <arg>namespace-uri-for-prefix(&quot;ha&quot;,/testcase/def:namespace-stuff/*[1])</arg>
    <output>http://sluggy.com
</output>
  </base>
  <base comment="test default namespace" type="pass">
    <arg>namespace-uri-for-prefix(&quot;&quot;,/testcase/def:namespace-stuff/*[1])   </arg>
    <output>http://defaultns.org/
</output>
  </base>
  <base comment="unknown prefix" type="pass">
    <arg>namespace-uri-for-prefix(&quot;ha&quot;,/testcase) </arg>
    <output/>
  </base>
  <base comment="null default NS" type="pass">
    <arg>namespace-uri-for-prefix(&quot;&quot;,/testcase) </arg>
    <output/>
  </base>
  <base type="pass">
    <arg>in-scope-prefixes(.)</arg>
    <output>xml
hee
</output>
  </base>
  <base type="pass">
    <arg>in-scope-prefixes(/)</arg>
    <output>ItemType matching failed [err:XPTY0004]
</output>
  </base>
  <base type="pass">
    <arg>in-scope-prefixes(@ls)</arg>
    <output>SequenceType matching failed: the sequence does not contain items [err:XPTY0004]
</output>
  </base>
  <base type="pass">
    <arg>in-scope-prefixes(/testcase/def:namespace-stuff)</arg>
    <output>xml

ha
ho
hee
</output>
  </base>
  <base comment="this test is correct but needs swhites new test thing" type="pass">
    <arg>in-scope-prefixes(/testcase/more-namespace-stuff)</arg>
    <output>xml
to
hee

</output>
  </base>
  <base type="pass">
    <arg>in-scope-prefixes(/testcase/more-namespace-stuff/to:inside)</arg>
    <output>xml

to
hee
</output>
  </base>
  <base type="pass">
    <arg>in-scope-prefixes(/testcase/more-namespace-stuff/to:inside/moreinside)</arg>
    <output>xml

to
hee
</output>
  </base>
</test>