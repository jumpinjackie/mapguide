<para name="para1" type="zzz">
    something
	<div><para name="para1b"/></div>
  </para>
<?xgrep-match [xgrep-test1.xml]--------------------------------?>
<para name="para2" type="warning"/>
<?xgrep-match [xgrep-test1.xml]--------------------------------?>
