            <Function name="{http://www.w3.org/2005/04/xpath-functions}:empty">
              <Navigation>
                <Variable name="c"/>
                <Step axis="child" name="intro" nodeType="element"/>
              </Navigation>
            </Function>
          </Where>
        </ForBinding>
        <Navigation>
          <Variable name="c"/>
          <Step axis="child" name="section" nodeType="element">
            <OQPlan>P(edge-element-presence-none,=,chapter.section)</OQPlan>
          </Step>
          <Step axis="child" name="intro" nodeType="element"/>
          <Step axis="child" name="para" nodeType="element">
            <OQPlan>P(edge-element-presence-none,=,intro.para)</OQPlan>
          </Step>
        </Navigation>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
