<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="results" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <LetBinding name="doc">
          <QueryPlanFunction result="document" container="11.5.8.dbxml">
            <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'prices.xml',U)</OQPlan>
          </QueryPlanFunction>
        </LetBinding>
        <ForBinding name="t">
          <Function name="{http://www.w3.org/2005/04/xpath-functions}:distinct-values">
            <Navigation>
              <Variable name="doc"/>
              <Step axis="descendant" name="book" nodeType="element"/>
              <Step axis="child" name="title" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,book.title)</OQPlan>
              </Step>
            </Navigation>
          </Function>
        </ForBinding>
        <LetBinding name="p">
          <Navigation>
            <Variable name="doc"/>
            <Step axis="descendant" name="book" nodeType="element"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="title" nodeType="element">
                  <RQPlan>V(book.title,=,'[to be calculated]')</RQPlan>
                </Step>
                <DbXmlCompare name="equal">
                  <Variable name="t"/>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
            <Step axis="child" name="price" nodeType="element"/>
          </Navigation>
        </LetBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="minprice" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Attributes>
            <DOMConstructor type="attribute">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="title" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </Name>
              <Children>
                <Variable name="t"/>
              </Children>
            </DOMConstructor>
          </Attributes>
          <Children>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="price" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </Name>
              <Children>
                <Function name="{http://www.w3.org/2005/04/xpath-functions}:min">
                  <Variable name="p"/>
                </Function>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
