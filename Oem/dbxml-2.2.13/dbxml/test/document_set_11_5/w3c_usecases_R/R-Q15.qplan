<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="u">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'users.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="user_tuple" nodeType="element"/>
          </Navigation>
        </ForBinding>
        <LetBinding name="b">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(bid_tuple.userid,=,'[to be calculated]'))</RQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="bid_tuple" nodeType="element">
              <Predicates>
                <Operator name="and">
                  <Navigation>
                    <Step axis="child" name="userid" nodeType="element">
                      <RQPlan>V(bid_tuple.userid,=,'[to be calculated]')</RQPlan>
                    </Step>
                    <DbXmlCompare name="equal">
                      <Navigation>
                        <Variable name="u"/>
                        <Step axis="child" name="userid" nodeType="element">
                          <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
                        </Step>
                      </Navigation>
                    </DbXmlCompare>
                  </Navigation>
                  <Navigation>
                    <Step axis="child" name="bid" nodeType="element"/>
                    <DbXmlCompare name="greater_than_equal">
                      <Sequence>
                        <AnyAtomicTypeConstructor value="100" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                      </Sequence>
                    </DbXmlCompare>
                  </Navigation>
                </Operator>
              </Predicates>
            </Step>
          </Navigation>
          <Where>
            <Operator name="greater_than">
              <Function name="{http://www.w3.org/2005/04/xpath-functions}:count">
                <Variable name="b"/>
              </Function>
              <Sequence>
                <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
              </Sequence>
            </Operator>
          </Where>
        </LetBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="big_spender" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="u"/>
              <Step axis="child" name="name" nodeType="element"/>
              <Step axis="child">
                <ItemType testType="text"/>
              </Step>
            </Navigation>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
