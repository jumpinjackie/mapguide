<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="frequent_bidder" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="u">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'users.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="user_tuple" nodeType="element"/>
          </Navigation>
          <Where>
            <EveryFLWOR>
              <ForBinding name="item">
                <Navigation>
                  <QueryPlanFunction result="document" container="11.5.3.dbxml">
                    <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',P(edge-element-equality-string,prefix,item_tuple.itemno))</OQPlan>
                  </QueryPlanFunction>
                  <Step axis="descendant" name="item_tuple" nodeType="element"/>
                </Navigation>
              </ForBinding>
              <SomeFLWOR>
                <ForBinding name="b">
                  <Navigation>
                    <QueryPlanFunction result="document" container="11.5.3.dbxml">
                      <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',n(V(bid_tuple.itemno,=,'[to be calculated]'),V(bid_tuple.userid,=,'[to be calculated]')))</RQPlan>
                    </QueryPlanFunction>
                    <Step axis="descendant" name="bid_tuple" nodeType="element"/>
                  </Navigation>
                </ForBinding>
                <Operator name="and">
                  <Navigation>
                    <Variable name="b"/>
                    <Step axis="child" name="itemno" nodeType="element">
                      <RQPlan>V(bid_tuple.itemno,=,'[to be calculated]')</RQPlan>
                    </Step>
                    <DbXmlCompare name="equal">
                      <Navigation>
                        <Variable name="item"/>
                        <Step axis="child" name="itemno" nodeType="element">
                          <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
                        </Step>
                      </Navigation>
                    </DbXmlCompare>
                  </Navigation>
                  <Navigation>
                    <Variable name="b"/>
                    <Step axis="child" name="userid" nodeType="element">
                      <RQPlan>V(bid_tuple.userid,=,'[to be calculated]')</RQPlan>
                    </Step>
                    <DbXmlCompare name="equal">
                      <Navigation>
                        <Variable name="u"/>
                        <Step axis="child" name="userid" nodeType="element">
                          <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
                        </Step>
                      </Navigation>
                    </DbXmlCompare>
                  </Navigation>
                </Operator>
              </SomeFLWOR>
            </EveryFLWOR>
          </Where>
        </ForBinding>
        <Navigation>
          <Variable name="u"/>
          <Step axis="child" name="name" nodeType="element"/>
        </Navigation>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
