<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="u">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'users.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="user_tuple" nodeType="element"/>
          </Navigation>
        </ForBinding>
        <LetBinding name="b">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(bid_tuple.userid,=,'[to be calculated]'))</RQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="bid_tuple" nodeType="element"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="userid" nodeType="element">
                  <RQPlan>V(bid_tuple.userid,=,'[to be calculated]')</RQPlan>
                </Step>
                <DbXmlCompare name="equal">
                  <Navigation>
                    <Variable name="u"/>
                    <Step axis="child" name="userid" nodeType="element">
                      <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
                    </Step>
                  </Navigation>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
        </LetBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <Navigation>
              <Variable name="u"/>
              <Step axis="child" name="userid" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
              </Step>
            </Navigation>
          </Specification>
        </Sort>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="user" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="u"/>
              <Step axis="child" name="userid" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,user_tuple.userid)</OQPlan>
              </Step>
            </Navigation>
            <Navigation>
              <Variable name="u"/>
              <Step axis="child" name="name" nodeType="element"/>
            </Navigation>
            <If>
              <Test>
                <Function name="{http://www.w3.org/2005/04/xpath-functions}:empty">
                  <Variable name="b"/>
                </Function>
              </Test>
              <Then>
                <DOMConstructor type="element">
                  <Name>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="status" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                    </Sequence>
                  </Name>
                  <Children>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="inactive" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                    </Sequence>
                  </Children>
                </DOMConstructor>
              </Then>
              <Else>
                <DOMConstructor type="element">
                  <Name>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="status" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                    </Sequence>
                  </Name>
                  <Children>
                    <Sequence>
                      <AnyAtomicTypeConstructor value="active" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                    </Sequence>
                  </Children>
                </DOMConstructor>
              </Else>
            </If>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
