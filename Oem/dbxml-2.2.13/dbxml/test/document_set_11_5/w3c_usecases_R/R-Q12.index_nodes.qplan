<XQuery>
  <FunctionDefinition name="{http://www.w3.org/2005/04/xquery-local-functions}:bid_summary">
    <FLWOR>
      <ForBinding name="i">
        <Function name="{http://www.w3.org/2005/04/xpath-functions}:distinct-values">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',P(edge-element-equality-string,prefix,itemno))</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="itemno" nodeType="element">
              <OQPlan>P(edge-element-equality-string,prefix,itemno)</OQPlan>
            </Step>
          </Navigation>
        </Function>
      </ForBinding>
      <LetBinding name="b">
        <Navigation>
          <QueryPlanFunction result="document" container="11.5.3.dbxml">
            <RQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'bids.xml',V(bid_tuple.itemno,=,'[to be calculated]'))</RQPlan>
          </QueryPlanFunction>
          <Step axis="descendant" name="bid_tuple" nodeType="element"/>
          <DbXmlFilter>
            <Navigation>
              <Step axis="child" name="itemno" nodeType="element">
                <RQPlan>V(bid_tuple.itemno,=,'[to be calculated]')</RQPlan>
              </Step>
              <DbXmlCompare name="equal">
                <Variable name="i"/>
              </DbXmlCompare>
            </Navigation>
          </DbXmlFilter>
        </Navigation>
      </LetBinding>
      <DOMConstructor type="element">
        <Name>
          <Sequence>
            <AnyAtomicTypeConstructor value="bid_count" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
          </Sequence>
        </Name>
        <Children>
          <DOMConstructor type="element">
            <Name>
              <Sequence>
                <AnyAtomicTypeConstructor value="itemno" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </Name>
            <Children>
              <Variable name="i"/>
            </Children>
          </DOMConstructor>
          <DOMConstructor type="element">
            <Name>
              <Sequence>
                <AnyAtomicTypeConstructor value="nbids" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </Name>
            <Children>
              <Function name="{http://www.w3.org/2005/04/xpath-functions}:count">
                <Variable name="b"/>
              </Function>
            </Children>
          </DOMConstructor>
        </Children>
      </DOMConstructor>
    </FLWOR>
  </FunctionDefinition>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="result" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <LetBinding name="bid_counts">
          <UserFunction name="{http://www.w3.org/2005/04/xquery-local-functions}:bid_summary">
          </UserFunction>
        </LetBinding>
        <LetBinding name="maxbids">
          <Function name="{http://www.w3.org/2005/04/xpath-functions}:max">
            <Navigation>
              <Variable name="bid_counts"/>
              <Step axis="child" name="nbids" nodeType="element"/>
            </Navigation>
          </Function>
        </LetBinding>
        <ForBinding name="item">
          <Navigation>
            <QueryPlanFunction result="document" container="11.5.3.dbxml">
              <OQPlan>D(node-metadata-equality-string,name:http://www.sleepycat.com/2002/dbxml,=,'items.xml',U)</OQPlan>
            </QueryPlanFunction>
            <Step axis="descendant" name="item_tuple" nodeType="element"/>
          </Navigation>
        </ForBinding>
        <ForBinding name="bc">
          <Navigation>
            <Variable name="bid_counts"/>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="nbids" nodeType="element"/>
                <DbXmlCompare name="equal">
                  <Variable name="maxbids"/>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
            <DbXmlFilter>
              <Navigation>
                <Step axis="child" name="itemno" nodeType="element"/>
                <DbXmlCompare name="equal">
                  <Navigation>
                    <Variable name="item"/>
                    <Step axis="child" name="itemno" nodeType="element">
                      <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
                    </Step>
                  </Navigation>
                </DbXmlCompare>
              </Navigation>
            </DbXmlFilter>
          </Navigation>
        </ForBinding>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="popular_item" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="item"/>
              <Step axis="child" name="itemno" nodeType="element">
                <OQPlan>P(edge-element-equality-string,prefix,item_tuple.itemno)</OQPlan>
              </Step>
            </Navigation>
            <Navigation>
              <Variable name="item"/>
              <Step axis="child" name="description" nodeType="element"/>
            </Navigation>
            <DOMConstructor type="element">
              <Name>
                <Sequence>
                  <AnyAtomicTypeConstructor value="bid_count" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
                </Sequence>
              </Name>
              <Children>
                <Navigation>
                  <Variable name="bc"/>
                  <Step axis="child" name="nbids" nodeType="element"/>
                  <Step axis="child">
                    <ItemType testType="text"/>
                  </Step>
                </Navigation>
              </Children>
            </DOMConstructor>
          </Children>
        </DOMConstructor>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
