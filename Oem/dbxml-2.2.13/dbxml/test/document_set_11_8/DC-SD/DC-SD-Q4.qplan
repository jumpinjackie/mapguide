<XQuery>
  <FLWOR>
    <LetBinding name="item">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>V(node-attribute-equality-string,@id,=,'I2')</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="catalog" nodeType="element"/>
        <Step axis="child" name="item" nodeType="element"/>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="id" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Sequence>
                <AnyAtomicTypeConstructor value="I2" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </LetBinding>
    <ForBinding name="prevItem">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>U</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="catalog" nodeType="element"/>
        <Step axis="child" name="item" nodeType="element">
          <Predicates>
            <Operator name="node-order">
              <ContextItem/>
              <Variable name="item"/>
            </Operator>
            <Operator name="equal">
              <Function name="{http://www.w3.org/2005/04/xpath-functions}:position"/>
              <Function name="{http://www.w3.org/2005/04/xpath-functions}:last"/>
            </Operator>
          </Predicates>
        </Step>
      </Navigation>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="CurrentItem" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="item"/>
              <Step axis="attribute" name="id" nodeType="attribute"/>
            </Navigation>
          </Children>
        </DOMConstructor>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="PreviousItem" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Navigation>
              <Variable name="prevItem"/>
              <Step axis="attribute" name="id" nodeType="attribute">
                <OQPlan>P(node-attribute-equality-string,prefix,@id)</OQPlan>
              </Step>
            </Navigation>
          </Children>
        </DOMConstructor>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
