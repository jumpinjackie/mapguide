<XQuery>
  <FLWOR>
    <ForBinding name="item">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <OQPlan>V(node-attribute-equality-string,@id,=,'I7')</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="catalog" nodeType="element"/>
        <Step axis="child" name="item" nodeType="element">
          <OQPlan>V(node-attribute-equality-string,@id,=,'I7')</OQPlan>
        </Step>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="id" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Sequence>
                <AnyAtomicTypeConstructor value="I7" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </ForBinding>
    <ForBinding name="related">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.2.dbxml">
          <RQPlan>V(@id,=,'[to be calculated]')</RQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="catalog" nodeType="element"/>
        <Step axis="child" name="item" nodeType="element">
          <RQPlan>V(@id,=,'[to be calculated]')</RQPlan>
        </Step>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="id" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Navigation>
                <Variable name="item"/>
                <Step axis="child" name="related_items" nodeType="element"/>
                <Step axis="child" name="related_item" nodeType="element"/>
                <Step axis="child" name="item_id" nodeType="element">
                  <OQPlan>P(node-element-equality-string,prefix,item_id)</OQPlan>
                </Step>
              </Navigation>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <Navigation>
          <Variable name="related"/>
          <Step axis="child" name="title" nodeType="element"/>
        </Navigation>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
