<XQuery>
  <FLWOR>
    <ForBinding name="ent">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.4.dbxml">
          <OQPlan>V(node-attribute-equality-string,@id,=,'E1')</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="dictionary" nodeType="element"/>
        <Step axis="child" name="e" nodeType="element">
          <OQPlan>V(node-attribute-equality-string,@id,=,'E1')</OQPlan>
        </Step>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="id" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Sequence>
                <AnyAtomicTypeConstructor value="E1" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </ForBinding>
    <ForBinding name="related">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.4.dbxml">
          <RQPlan>V(@id,=,'[to be calculated]')</RQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="dictionary" nodeType="element"/>
        <Step axis="child" name="e" nodeType="element">
          <RQPlan>V(@id,=,'[to be calculated]')</RQPlan>
        </Step>
        <DbXmlFilter>
          <Navigation>
            <Step axis="attribute" name="id" nodeType="attribute"/>
            <DbXmlCompare name="equal">
              <Navigation>
                <Variable name="ent"/>
                <Step axis="child" name="et" nodeType="element"/>
                <Step axis="child" name="cr" nodeType="element">
                  <OQPlan>P(node-element-equality-string,prefix,cr)</OQPlan>
                </Step>
              </Navigation>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </ForBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <Navigation>
          <Variable name="related"/>
          <Step axis="child" name="hwg" nodeType="element"/>
          <Step axis="child" name="hw" nodeType="element">
            <OQPlan>P(node-element-equality-string,prefix,hw)</OQPlan>
          </Step>
        </Navigation>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
