<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.3.dbxml">
          <OQPlan>n(V(node-element-substring-string,p,=,'hoc'),V(node-element-substring-string,p,=,'ock'),V(node-element-substring-string,p,=,'cke'),V(node-element-substring-string,p,=,'key'))</OQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="article" nodeType="element"/>
      </Navigation>
      <Where>
        <EveryFLWOR>
          <ForBinding name="b">
            <Navigation>
              <Variable name="a"/>
              <Step axis="child" name="body" nodeType="element"/>
              <Step axis="child" name="abstract" nodeType="element"/>
              <Step axis="child" name="p" nodeType="element"/>
            </Navigation>
          </ForBinding>
          <Navigation>
            <Variable name="b"/>
            <DbXmlContains>
              <OQPlan>n(V(node-element-substring-string,p,=,'hoc'),V(node-element-substring-string,p,=,'ock'),V(node-element-substring-string,p,=,'cke'),V(node-element-substring-string,p,=,'key'))</OQPlan>
              <Sequence>
                <AnyAtomicTypeConstructor value="hockey" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
              </Sequence>
            </DbXmlContains>
          </Navigation>
        </EveryFLWOR>
      </Where>
    </ForBinding>
    <Navigation>
      <Variable name="a"/>
      <Step axis="child" name="prolog" nodeType="element"/>
      <Step axis="child" name="title" nodeType="element"/>
    </Navigation>
  </FLWOR>
</XQuery>
