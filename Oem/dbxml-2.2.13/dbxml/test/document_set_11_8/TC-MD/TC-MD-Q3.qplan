<XQuery>
  <FLWOR>
    <ForBinding name="a">
      <Function name="{http://www.w3.org/2005/04/xpath-functions}:distinct-values">
        <Navigation>
          <QueryPlanFunction result="collection" container="11.8.3.dbxml">
            <OQPlan>P(node-element-equality-string,prefix,date)</OQPlan>
          </QueryPlanFunction>
          <Step axis="child" name="article" nodeType="element"/>
          <Step axis="child" name="prolog" nodeType="element"/>
          <Step axis="child" name="dateline" nodeType="element"/>
          <Step axis="child" name="date" nodeType="element"/>
        </Navigation>
      </Function>
    </ForBinding>
    <LetBinding name="b">
      <Navigation>
        <QueryPlanFunction result="collection" container="11.8.3.dbxml">
          <RQPlan>V(date,=,'[to be calculated]')</RQPlan>
        </QueryPlanFunction>
        <Step axis="child" name="article" nodeType="element"/>
        <Step axis="child" name="prolog" nodeType="element"/>
        <Step axis="child" name="dateline" nodeType="element"/>
        <DbXmlFilter>
          <Navigation>
            <Step axis="child" name="date" nodeType="element"/>
            <DbXmlCompare name="equal">
              <Variable name="a"/>
            </DbXmlCompare>
          </Navigation>
        </DbXmlFilter>
      </Navigation>
    </LetBinding>
    <DOMConstructor type="element">
      <Name>
        <Sequence>
          <AnyAtomicTypeConstructor value="Output" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
        </Sequence>
      </Name>
      <Children>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="Date" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Variable name="a"/>
          </Children>
        </DOMConstructor>
        <DOMConstructor type="element">
          <Name>
            <Sequence>
              <AnyAtomicTypeConstructor value="NumberOfArticles" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
            </Sequence>
          </Name>
          <Children>
            <Function name="{http://www.w3.org/2005/04/xpath-functions}:count">
              <Variable name="b"/>
            </Function>
          </Children>
        </DOMConstructor>
      </Children>
    </DOMConstructor>
  </FLWOR>
</XQuery>
