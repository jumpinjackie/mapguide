<XQuery>
  <DOMConstructor type="element">
    <Name>
      <Sequence>
        <AnyAtomicTypeConstructor value="parttree" typeuri="http://www.w3.org/2001/XMLSchema" typename="string"/>
      </Sequence>
    </Name>
    <Children>
      <FLWOR>
        <ForBinding name="p">
          <Navigation>
            <QueryPlanFunction result="collection" container="11.7.3.dbxml">
              <OQPlan>R(edge-attribute-equality-decimal,part.@id,>=,'1',<=,'10')</OQPlan>
            </QueryPlanFunction>
            <Step axis="child" name="partlist" nodeType="element"/>
            <Step axis="child" name="part" nodeType="element">
              <Predicates>
                <Operator name="and">
                  <Navigation>
                    <Step axis="attribute" name="id" nodeType="attribute">
                      <OQPlan>V(edge-attribute-equality-decimal,part.@id,>=,'1')</OQPlan>
                    </Step>
                    <DbXmlCompare name="greater_than_equal">
                      <Sequence>
                        <AnyAtomicTypeConstructor value="1" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                      </Sequence>
                    </DbXmlCompare>
                  </Navigation>
                  <Navigation>
                    <Step axis="attribute" name="id" nodeType="attribute">
                      <OQPlan>V(edge-attribute-equality-decimal,part.@id,<=,'10')</OQPlan>
                    </Step>
                    <DbXmlCompare name="less_than_equal">
                      <Sequence>
                        <AnyAtomicTypeConstructor value="10" typeuri="http://www.w3.org/2001/XMLSchema" typename="integer"/>
                      </Sequence>
                    </DbXmlCompare>
                  </Navigation>
                </Operator>
              </Predicates>
            </Step>
          </Navigation>
        </ForBinding>
        <Sort>
          <Specification modifier="ascending|empty_least">
            <Navigation>
              <Variable name="p"/>
              <Step axis="attribute" name="id" nodeType="attribute">
                <OQPlan>P(edge-attribute-equality-decimal,prefix,part.@id)</OQPlan>
              </Step>
            </Navigation>
          </Specification>
        </Sort>
        <Variable name="p"/>
      </FLWOR>
    </Children>
  </DOMConstructor>
</XQuery>
