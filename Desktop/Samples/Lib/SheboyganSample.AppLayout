          </ItemBase>
          <ItemBase xsi:type="SeparatorItem" />
          <ItemBase xsi:type="CommandItem">
            <ComponentID>ViewerOptions</ComponentID>
            <ShowLabel>false</ShowLabel>
          </ItemBase>
        </Items>
      </ItemBase>
    </Items>
